    <DataSet Name="dsMissingIndexes">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as 
(
select 
	count(1) [missing]
	,'- ' +[statement]+' : ' [state]
from sys.dm_db_missing_index_details
where database_id=db_id()
group by statement 
)
select 
	isnull(sum(missing),0)  [missing]
	,STRING_AGG([state]+convert(varchar(10),missing),char(10)) [objs]
from data 
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="missing">
          <DataField>missing</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="objs">
          <DataField>objs</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsXESessions">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@action">
            <Value>=Parameters!xEvent_Action.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@name">
            <Value>=Parameters!xEventsTrace.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*  create xEvent trace session if it's not present*/

declare @cmd varchar(512)
set @cmd='CREATE EVENT SESSION [xEtrace_Errors] ON DATABASE 
ADD EVENT sqlserver.error_reported(
    ACTION(sqlserver.client_app_name,sqlserver.client_hostname,sqlserver.plan_handle,sqlserver.query_hash,sqlserver.session_id,sqlserver.sql_text,sqlserver.tsql_stack,sqlserver.username))
ADD TARGET package0.ring_buffer(SET max_memory=(204800), max_events_limit = 50000)' 
if not exists (select 1 from sys.database_event_sessions where name='xEtrace_Errors')
	begin
		exec (@cmd)
set @cmd=''
	end

/*enable/disable xEvent trace session*/
if @action in (0,1) and @name is not null  /*0 - stop, 1 - start*/
   begin

	select  @cmd=
	 case 
		 when (exists (select 1 from sys.dm_xe_database_sessions where name=@name) and @action=0) then 'ALTER EVENT SESSION '+@name+' ON DATABASE STATE = STOP'
		 when (not exists (select 1 from sys.dm_xe_database_sessions where name=@name) and @action=1) then 'ALTER EVENT SESSION '+@name+' ON DATABASE STATE = START'
		 else '-- do nothing' end

        exec (@cmd)
      set @cmd=''

   end 


;with ev as 
(
select 
	event_session_id
	,count(1) [n]
	,STRING_AGG('- '+name,char(10)) WITHIN GROUP ( ORDER BY [name] asc) [eventnames]
from sys.database_event_session_events
group by event_session_id
)

 select 
	es.event_session_id [id]
	,es.name
	,es.startup_state [startup]
	,convert(smalldatetime,ds.create_time) [status]
	,case when datediff(ss,ds.create_time,getdate())&gt;=86400 then convert(varchar(10),datediff(dd,ds.create_time,getdate()))+'d ' else '' end
		+convert(varchar(20),dateadd(ss,datediff(ss,ds.create_time,getdate()),0),108) [duration]
	,STRING_AGG(st.name,', ') WITHIN GROUP ( ORDER BY st.name asc)  [targets]
	,ev.n
	,ev.eventnames
 	,ds.total_bytes_generated [bytes]
 from sys.database_event_sessions  es
 left join sys.dm_xe_database_sessions ds on es.name=ds.name
 join sys.database_event_session_targets st on es.event_session_id=st.event_session_id
 join ev on es.event_session_id=ev.event_session_id
 group by 
	es.event_session_id,
	es.name, es.startup_state,ds.create_time 
	,ev.n, ev.eventnames
	,ds.total_bytes_generated
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="startup">
          <DataField>startup</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="duration">
          <DataField>duration</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="targets">
          <DataField>targets</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="n">
          <DataField>n</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="eventnames">
          <DataField>eventnames</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="bytes">
          <DataField>bytes</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsUtilization">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with ttl as 
(
	select
		 (count(end_time) - sum(case when avg_cpu_percent &gt; 80 then 1 else 0 end) * 1.0) / count(end_time)*100 [CPUFit]
		,(count(end_time) - sum(case when avg_log_write_percent &gt; 80 then 1 else 0 end) * 1.0) / count(end_time)*100 [LogWriteFit]
		,(count(end_time) - sum(case when avg_data_io_percent &gt; 80 then 1 else 0 end) * 1.0) / count(end_time)*100 [PhysicalDataReadFit]
	FROM sys.dm_db_resource_stats
)
select 
	[CPUFit]
	,[LogWriteFit]
	,[PhysicalDataReadFit]
	,(select min(vals) from (values([CPUFit]),([LogWriteFit]),([PhysicalDataReadFit])) t(vals)) [min]
from ttl 
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="CPUFit">
          <DataField>CPUFit</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="LogWriteFit">
          <DataField>LogWriteFit</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PhysicalDataReadFit">
          <DataField>PhysicalDataReadFit</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Min">
          <DataField>Min</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
