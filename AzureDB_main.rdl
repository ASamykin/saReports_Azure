    <DataSet Name="dsServerInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	@@servername [server]
	,db_name() [DB]
	,getdate() [time]
	, edition
	, service_objective [service]
