    <DataSet Name="dsProcCache">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	select  objtype [objtype]
			, cacheobjtype
			, sum(1) [plans]
			, convert(numeric(10,1),sum(size_in_bytes/1024.)/1024.) [SizeMB]

			, max(size_in_bytes)/1024 [MaxPlan] -- KB

			, floor(avg(convert(numeric(18,2),usecounts))) [UseCountAll_AVG]
			, sum(case when usecounts = 1 THEN 1 else 0 end) [UseCount1_Amount]
			, convert(numeric(5,2),sum(case when usecounts = 1 THEN 1. else 0. end)/sum(1)*100) [UseCount1_Percent]
			, convert(numeric(10,2),sum(cast((CASE WHEN usecounts = 1 THEN size_in_bytes ELSE 0 END) as decimal(18,2)))/1024./1024.) AS [USECount1_MB]
	from sys.dm_exec_cached_plans 
	group by objtype, cacheobjtype
	order by 4 desc
option (recompile)</CommandText>
