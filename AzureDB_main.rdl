    <DataSet Name="dsUsers">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	replace(type_desc,'EXTERNAL_','EXT_') [type]
	,convert(varchar(20),count(1)) [num]
	,'Names: '+char(10)+STRING_AGG('- ' + name,char(10)) [names]
