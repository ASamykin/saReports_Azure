    <DataSet Name="dsResources">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@keep">
            <Value>=Parameters!KeepHistory.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set nocount on;
declare @cmd varchar(1024)
declare @all varchar(1024)
--declare @keep tinyint = 1

set @cmd = 'select 
	end_time [time]
	,avg_cpu_percent [cpu]
	,avg_data_io_percent [io]
	,avg_log_write_percent [logwrite]
	,avg_memory_usage_percent [memory]
into ##buffer
from sys.dm_db_resource_stats'

if @keep = 1   /*check report variable*/
	begin 

		set @all = 'select 
			[time]
			,[cpu]
			,[io]
			,[logwrite]
			,[memory]
		from ##buffer'

		if object_id('tempdb..##buffer') is null
			begin
				exec (@cmd)
				exec ('create index idx_1 on ##buffer([time]);')
			end
		else 
			begin
				exec('insert into ##buffer ([time],[cpu],[io],[logwrite],[memory]) select end_time,avg_cpu_percent,avg_data_io_percent,avg_log_write_percent,avg_memory_usage_percent from sys.dm_db_resource_stats where end_time not in (select [time] from ##buffer) option (recompile);')
			end
	end
else
	begin
		set @all = replace(@cmd,'into ##buffer','')
	end

exec (@all)</CommandText>
      </Query>
      <Fields>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cpu">
          <DataField>cpu</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="io">
          <DataField>io</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="logwrite">
          <DataField>logwrite</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="memory">
          <DataField>memory</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCPURAM">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
(select cntr_value/1024 from sys.dm_os_performance_counters where counter_name ='Target Server Memory (KB)') [TargetMemoryMB]
,(select cntr_value/1024 from sys.dm_os_performance_counters where counter_name ='Total Server Memory (KB)') [TotalMemoryMB]
,(select case when cntr_value&gt;86400 then convert(varchar(10),floor(cntr_value/86400))+'d ' else '' end + convert(varchar(20),dateadd(ss,cntr_value,0),108) from sys.dm_os_performance_counters where counter_name ='Page life expectancy' and object_name like '%:Buffer Manager%') [PLE]
	,sum(case when status='VISIBLE ONLINE' then 1 else 0 end) [CPUVisible]
	,sum(case when status='HIDDEN ONLINE' then 1 else 0 end) [CPUHidden]
	,sum(case when status not in ('VISIBLE ONLINE','HIDDEN ONLINE','VISIBLE ONLINE (DAC)') then 1 else 0 end) [CPUOthers]
from sys.dm_os_schedulers
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="TargetMemoryMB">
          <DataField>TargetMemoryMB</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="TotalMemoryMB">
          <DataField>TotalMemoryMB</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="PLE">
          <DataField>PLE</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CPUVisible">
          <DataField>CPUVisible</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CPUHidden">
          <DataField>CPUHidden</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CPUOthers">
          <DataField>CPUOthers</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDBInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with dbs as 
	(select 'tempdb' [db] union all
	 select physical_database_name from sys.databases )
,data as (
select 
	case when rtrim(instance_name)&lt;&gt;'tempdb' then db_name() else instance_name end [db]
	,counter_name
	,cntr_value/1024 [cntr_value]
from sys.dm_os_performance_counters
where OBJECT_NAME like '%Databases%'
and counter_name like '%File(s)%Size%'
and instance_name in (select db from dbs)

union all

select 
	db_name()
	,'Data File(s) Used Size (KB)'
 	,sum(CAST(FILEPROPERTY(name, 'SpaceUsed') AS bigint)  / 128)  [UsedMB]
FROM sys.database_files
where type=0 /*ROWS*/

union all 

select case when database_id=4 then 'msdb' else db_name(database_id) end [DB]
	,'Total Files'
	,count(1) [files]
FROM sys.dm_io_virtual_file_stats(NULL, NULL)
where db_name(database_id) in ('tempdb',db_name())
group by database_id
)
select 
	rtrim(ltrim(db)) [db]
	,rtrim(replace(counter_name,' Size (KB)',' (MB)')) [counter]
	,cntr_value [value]
from data 

option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="counter">
          <DataField>counter</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
