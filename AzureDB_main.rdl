    <DataSet Name="dsDBConfig">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
select 
	'Non-default settings' [name]
	,count(1) [num]
	,'Amended settings:'+char(10)+STRING_AGG(' - '+name+': '+convert(varchar(20),[value]),char(10)) [desc]
from sys.database_scoped_configurations
where is_value_default&lt;&gt;1
option (recompile)
*/

select 
	'Non-default settings' [name]
	,sum(case when is_value_default=1 then 0 else 1 end) [num]
	,'Amended settings:'+char(10)
		+STRING_AGG(case 
					when is_value_default&lt;&gt;1 then ' - '+name+': '+convert(varchar(20),[value]) 
					else '' 
					end ,char(10)) [changed]
	,'Default settings:'+char(10)
		+STRING_AGG(case 
					when is_value_default=1 then ' - '+name+': '+convert(varchar(20),[value]) 
					else '' 
					end ,char(10)) [default]
from sys.database_scoped_configurations
order by name
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="num">
          <DataField>num</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="changed">
