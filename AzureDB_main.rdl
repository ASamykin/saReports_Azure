          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="pool">
          <DataField>pool</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="dtu">
          <DataField>dtu</DataField>
          <rd:UserDefined>true</rd:UserDefined>
