    <DataSet Name="dsConnections">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as 
(
select 
	des.session_id [spid]
	,des.host_name [hostname]
	,des.program_name
	,isnull(der.command,'AWAITING COMMAND') [cmd]
	,des.login_name [loginame]
	,des.status
	,isnull(der.blocking_session_id,0) [blocked]
	,des.last_request_end_time [last_batch]
from sys.dm_exec_sessions des
left join sys.dm_exec_requests der on des.session_id=der.session_id
where des.is_user_process=1
and des.database_id=db_id()
)
select 
	count(distinct spid) [connected]
	,count(distinct loginame) [uniqueusers]
	,count(distinct hostname) [uniquehosts]
	,count(distinct [program_name]) [uniqueapp]
	,sum(case when cmd&lt;&gt;'AWAITING COMMAND' then 1 else 0 end) [activesessions]
	,sum(case when blocked&lt;&gt;0 then 1 else 0 end) [blockedsessions]
	,max(last_batch) [lastquery]

	,(select '- '+STRING_AGG(loginame2,char(10)+'- ') from (select  rtrim(loginame) +' '+quotename(convert(varchar(20),count(1))) [loginame2] from data group by loginame) a ) [users]
	,(select '- '+STRING_AGG(hosts,char(10)+'- ') from (select  rtrim(hostname) +' '+quotename(convert(varchar(20),count(1))) [hosts] from data group by hostname) a ) [hosts]
	,(select '- '+STRING_AGG(apps,char(10)+'- ') from (select  rtrim([program_name]) +' '+quotename(convert(varchar(20),count(1))) [apps] from data group by [program_name]) a ) [apps]
	,(select '- '+STRING_AGG(cmds,char(10)+'- ') from (select  rtrim([cmd]) +' '+quotename(convert(varchar(20),count(1))) [cmds] from data group by [cmd]) a ) [cmds]
	,(select '- '+STRING_AGG([block],char(10)+'- ') from (select  convert(varchar(20),[blocked]) +' '+quotename(convert(varchar(20),count(1))) [block] from data where [blocked]!=0 group by [blocked]) a ) [blocked]

from data
option (recompile)
</CommandText>
      </Query>
      <Fields>
        <Field Name="connected">
          <DataField>connected</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="uniqueusers">
          <DataField>uniqueusers</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="uniquehosts">
          <DataField>uniquehosts</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="uniqueapp">
          <DataField>uniqueapp</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="activesessions">
          <DataField>activesessions</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="blockedsessions">
          <DataField>blockedsessions</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="lastquery">
          <DataField>lastquery</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="users">
          <DataField>users</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="hosts">
          <DataField>hosts</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="apps">
