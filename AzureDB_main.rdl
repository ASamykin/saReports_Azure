    <DataSet Name="dsServerInfo">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	@@servername [server]
	,db_name() [DB]
	,getdate() [time]
	, edition
	, service_objective [service]
	, elastic_pool_name [pool]
	,(select top 1 dtu_limit from sys.dm_db_resource_stats order by end_time desc ) [dtu]
from sys.database_service_objectives</CommandText>
      </Query>
      <Fields>
        <Field Name="server">
          <DataField>server</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
