    <DataSet Name="dsTuningRecommends">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
;with data as 
(
	select
		ddtr.type [Type]
		,JSON_VALUE(ddtr.state,'$.currentValue') [CurrentState]
		,JSON_VALUE(ddtr.state,'$.reason') [CurrentStateReason]
		,ddtr.execute_action_initiated_by [by]
		,count(1) [n]
	from sys.dm_db_tuning_recommendations ddtr
	cross apply  openjson(ddtr.details,'$.planForceDetails')
			WITH (query_id INT '$.queryId',
				  regressedPlanId INT '$.regressedPlanId',
				  recommendedPlanId INT '$.recommendedPlanId') 
	group by 
		ddtr.type
		,JSON_VALUE(ddtr.state,'$.currentValue')
		,JSON_VALUE(ddtr.state,'$.reason')
		,ddtr.execute_action_initiated_by
)
select 
	[type]
	,sum(n) [amount]
	,STRING_AGG(' - '+[Type]+': '+[CurrentState] + ' '+quotename(convert(varchar(10),[n]))+ ' with reason "'+[CurrentStateReason]+'"'+isnull(' implemented by '+[by],''),char(10)) [desc]
from data 
group by [type]
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsProcCacheDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with SizeLeaders as (
select 
	ROW_NUMBER() over (partition by objtype,cacheobjtype order by size_in_bytes desc) n
	,objtype,cacheobjtype,size_in_bytes
	,usecounts,refcounts,plan_handle
from sys.dm_exec_cached_plans 
)
, OtherSameSize as 
(select 
	objtype,cacheobjtype,size_in_bytes,count(1) [amount]
from sys.dm_exec_cached_plans 
group by objtype,cacheobjtype,size_in_bytes
)
select 
	b.objtype
	,b.cacheobjtype
	,b.size_in_bytes/1024 [sizeKB]
	,b.usecounts
	,b.refcounts
	,d.amount [PlansSameSize]
	,case when t.dbid=32767 then 'mssqlsystemresource' else db_name(t.dbid) end [db]
	,isnull(object_name(t.objectid,t.dbid),left(t.text,1000)) [ObjectQuery]
from SizeLeaders b
join OtherSameSize d on (b.objtype=d.objtype and b.cacheobjtype=d.cacheobjtype and b.size_in_bytes=d.size_in_bytes)
outer apply sys.dm_exec_sql_text(b.plan_handle) t
where b.n=1
option (recompile,maxdop 1)</CommandText>
      </Query>
