		+STRING_AGG(case 
					when is_value_default&lt;&gt;1 then ' - '+name+': '+convert(varchar(20),[value]) 
					end ,char(10)) WITHIN GROUP ( ORDER BY name) [changed]
	,'Default settings:'+char(10)
		+STRING_AGG(case 
					when is_value_default=1 then ' - '+name+': '+convert(varchar(20),[value]) 
 
					end ,char(10))  WITHIN GROUP ( ORDER BY name)  [default]
from sys.database_scoped_configurations
order by name
option (recompile)</CommandText>
