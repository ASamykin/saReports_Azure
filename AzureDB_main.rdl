      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
select 
	'Non-default settings' [name]
	,count(1) [num]
	,'Amended settings:'+char(10)+STRING_AGG(' - '+name+': '+convert(varchar(20),[value]),char(10)) within group (order by [name]) [desc]
from sys.database_scoped_configurations
where is_value_default&lt;&gt;1
