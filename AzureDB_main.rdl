    <DataSet Name="dsTempDBUsed">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
SELECT 
	
	tsu.session_id [spid]
	,es.login_name [login]
	,es.program_name [application]
	,isnull(case when es.host_name = '' then NULL else es.host_name end,dec.client_net_address)  [host]
    ,SUBSTRING(st.text, er.statement_start_offset/2 + 1
                        ,(CASE WHEN er.statement_end_offset = -1 
                               THEN LEN(CONVERT(nvarchar(max),st.text))*2 
                               ELSE er.statement_end_offset
                          END - er.statement_start_offset)/2
                ) [query]
     ,sum(tsu.user_objects_alloc_page_count - tsu.user_objects_dealloc_page_count) [OutStandingUserObjectsPages]
     ,sum(tsu.internal_objects_alloc_page_count - tsu.internal_objects_dealloc_page_count) [OutStandingInternalObjectsPages]
     ,min(er.start_time) [start]
	 ,max(er.command) [command]
	 ,avg(er.open_transaction_count) [opentran]
     ,max(er.cpu_time) [cpu]
	 ,avg(er.total_elapsed_time) [elapsed]
	 ,max(er.reads) [reads]
	 ,max(er.writes) [writes]
     ,max(er.logical_reads) [logicalreads]
	 ,max(er.granted_query_memory)/128. [GrantedQueryMemory]
,convert(varchar(20),dateadd(ss,datediff(ss,min(er.start_time),getdate()),0),108) [running]
FROM tempdb.sys.dm_db_task_space_usage tsu  
JOIN sys.dm_exec_requests er 
    ON tsu.session_id = er.session_id 
   AND tsu.request_id = er.request_id  
JOIN sys.dm_exec_sessions es 
    ON tsu.session_id = es.session_id  
join sys.dm_exec_connections dec on er.session_id=dec.session_id 
CROSS APPLY sys.dm_exec_sql_text(er.sql_handle) st 
WHERE (tsu.internal_objects_alloc_page_count 
      +tsu.user_objects_alloc_page_count) &gt; 0 
and tsu.session_id&lt;&gt;@@spid
group by 
	isnull(case when es.host_name = '' then NULL else es.host_name end,dec.client_net_address) 
	,es.login_name
	,es.program_name
    ,SUBSTRING(st.text, er.statement_start_offset/2 + 1
                        ,(CASE WHEN er.statement_end_offset = -1 
                               THEN LEN(CONVERT(nvarchar(max),st.text))*2 
                               ELSE er.statement_end_offset
                          END - er.statement_start_offset)/2
                )
	,tsu.session_id 
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="spid">
          <DataField>spid</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="login">
          <DataField>login</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="application">
          <DataField>application</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="host">
          <DataField>host</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="query">
          <DataField>query</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="OutStandingUserObjectsPages">
          <DataField>OutStandingUserObjectsPages</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="OutStandingInternalObjectsPages">
          <DataField>OutStandingInternalObjectsPages</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="start">
