  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsStats">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@obj">
            <Value>=Parameters!TableName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with cols as 
(
select 
	sc.object_id
	,sc.stats_id
	,count(1) [cols]
	,STRING_AGG(convert(varchar(20),sc.stats_column_id)+') '+c.name,char(10)) WITHIN GROUP (ORDER BY sc.stats_column_id ASC) [colnames]
from sys.stats_columns sc
join sys.columns c on sc.column_id=c.column_id and sc.object_id=c.object_id
group by sc.object_id,sc.stats_id

)
SELECT 
	sp.stats_id [id]

	, s.name
	, s.auto_created [AutoCreated]
	, s.user_created [UserCreated]
	, s.no_recompute [Auto]
	, s.filter_definition [Filter]

	, sp.last_updated [Updated]
	, datediff(ss,sp.last_updated,getdate()) [Updated2]
	, sp.rows [Rows]
	, sp.rows_sampled [SampledRows]
	, 1.*sp.rows_sampled/sp.rows*100 [SamplePerc]
	, sp.steps [Steps]
	, sp.unfiltered_rows [UnFilteredRows]
	, case when sp.unfiltered_rows=sp.rows then 'nf' else 'f' end [StatsType]
	, sp.modification_counter   [ModifiedRecs]
	, 1.*sp.modification_counter/sp.rows*100 [ModifiedRecsPerc]

	,c.cols
	,c.colnames
FROM sys.stats s  
join cols c on s.object_id=c.object_id and s.stats_id=c.stats_id
CROSS APPLY sys.dm_db_stats_properties(s.object_id, s.stats_id) sp
WHERE s.object_id = object_id(@obj)
option (recompile)</CommandText>
      </Query>
