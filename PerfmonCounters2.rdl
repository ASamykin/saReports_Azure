  <DataSets>
    <DataSet Name="dsHitRatio">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt">
            <Value>=Parameters!ShowDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@time">
            <Value>=Parameters!ShowTime.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set nocount on;
/*
declare @srv varchar(128) =  'sw20dwh100a'
declare @db varchar(128)  =  'EDW'
declare @dt varchar(10) = '04-26-2019'
declare @time varchar(10) = ' '--' 13:05'

*/
declare @obj nvarchar(50) 
declare @dt1 datetime
declare @dt2 datetime

select top 1
	@obj=left([object_name],iif(charindex(':',[object_name])=0,0,charindex(':',[object_name])-1))
from sys.dm_os_performance_counters
order by 1 desc


select @dt1=convert(datetime,@dt+@time)

if @time=' '
	begin /*whole day*/
		select @dt2= dateadd(dd,1,@dt1)
	end
else
	begin  /*special time in a day*/
		select @dt2=dateadd(mi,1,@dt1)
	end

;with hits (dt,counter_name,[value]) 
as 
	(
select [dt],[counter_name],[value]
from ##SSMSReports_TempData_DBA
where 
  ((object_name=@obj+N':Buffer Manager'  and counter_name=N'Buffer cache hit ratio')
or 
(object_name=@obj+N':Plan Cache'  and counter_name=N'Cache Hit Ratio' and instance_name=N'_Total'))
and (dt&gt;=@dt1 and dt&lt;@dt2)
	)
select 
    dt, [Buffer cache hit ratio] [buffer], [Cache Hit Ratio] [cache]
from hits as h
pivot (max([value]) for [counter_name] in ([Buffer cache hit ratio],[Cache Hit Ratio])) as p
order by dt
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="buffer">
          <DataField>buffer</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cache">
          <DataField>cache</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsConnectionsTransPLE">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt">
            <Value>=Parameters!ShowDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@time">
            <Value>=Parameters!ShowTime.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set nocount on;

/*
declare @srv nvarchar(128) =  N'sw20dwh100a'
declare @db nvarchar(128)  =  N'EDW'
*/

declare @obj nvarchar(50) 
declare @dt1 datetime
declare @dt2 datetime

/*
declare @dt varchar(10) = '04-26-2019'
declare @time varchar(10) = ' '--' 13:05'
*/


select @dt1=convert(datetime,@dt+@time)

if @time=' '
	begin /*whole day*/
		select @dt2= dateadd(dd,1,@dt1)
	end
else
	begin  /*special time in a day*/
		select @dt2=dateadd(mi,1,@dt1)
	end
	
;with data (dt,counter_name,[value]) as 
	(
select
	dt
	,rtrim(counter_name)
	,[value]
from ##SSMSReports_TempData_DBA
where 
	counter_name in ('Page life expectancy','Active Temp Tables','User Connections','Transactions','Processes blocked')
and (dt&gt;=@dt1 and dt&lt;@dt2)
	)
select 
	dt
	,[Page life expectancy] [PLE]
                   ,[Active Temp Tables] [ActTempTables]
                   ,[User Connections] [conns]
                   ,[Transactions] [trans]
                   ,[Processes blocked] [blocked]
from data as d
pivot (max([value]) for [counter_name] in ([Page life expectancy],[Active Temp Tables],[User Connections],[Transactions],[Processes blocked])) as p
order by dt
option (recompile)</CommandText>
      </Query>
