  <DataSets>
    <DataSet Name="dsPlansPerHour">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with data as (
select 
	convert(smalldatetime,dateadd(hh,datediff(hh,0,s.creation_time),0)) [dt]
	,p.objtype+':'+p.cacheobjtype [type]
	,count(distinct s.plan_handle) [plans]
from sys.dm_exec_query_stats s
join sys.dm_exec_cached_plans p on s.plan_handle=p.plan_handle
group by 
	dateadd(hh,datediff(hh,0,s.creation_time),0),
	p.objtype+':'+p.cacheobjtype
)
select 
	ROW_NUMBER() over (order by dt asc) [n]
	,[dt]
	,[type]
	,plans 
from data 
option (recompile, maxdop 1)</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="plans">
          <DataField>plans</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="day">
          <Value>=format(Fields!dt.Value,"dd MMM")</Value>
        </Field>
        <Field Name="time">
          <Value>=format(Fields!dt.Value,"HH:mm")</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
