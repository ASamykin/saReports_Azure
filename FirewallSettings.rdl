    </DataSet>
    <DataSet Name="dsFirewallRules">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>declare @cmd varchar(1000)

set @cmd='
select 
	''Database'' [level]
	,id
	,name
	,start_ip_address
	,end_ip_address
	,create_date
	,modify_date
from sys.database_firewall_rules 
'
+ case when db_name()='master' then 
'
union all

select 
	''Server'' [level]
	,id
	,name
	,start_ip_address
	,end_ip_address
	,create_date
	,modify_date
from sys.firewall_rules 
' 
else '' end
exec (@cmd)</CommandText>
      </Query>
