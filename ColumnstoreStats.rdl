  <DataSets>
    <DataSet Name="dsColumnstoreStats">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!ObjectID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT 
	object_schema_name(p.object_id) [schema]
	,object_name(p.object_id) [table]
	,p.index_id
	,i.name [indexname]
	,i.type_desc [type]
	,p.partition_number
	,p.row_group_id
	,p.generation
	,p.state_desc
	,p.has_vertipaq_optimization
	,p.total_rows
	,p.deleted_rows
	,100*(ISNULL(p.deleted_rows,0))/p.total_rows [fragmentation]
	,p.size_in_bytes
	,p.trim_reason_desc
	,p.transition_to_compressed_state_desc
	,p.created_time
	,p.closed_time
FROM sys.dm_db_column_store_row_group_physical_stats p
join sys.indexes i on i.object_id = p.object_id AND i.index_id = p.index_id  
where p.object_id=@id
option (recompile)</CommandText>
      </Query>
