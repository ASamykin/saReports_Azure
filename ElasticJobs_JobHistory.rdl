    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
