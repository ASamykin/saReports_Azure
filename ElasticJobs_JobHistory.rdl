    </DataSet>
    <DataSet Name="dsJobHistory">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@jobname">
            <Value>=Parameters!Job.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@date">
            <Value>=Parameters!DT.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	--j.job_execution_id
	abs(checksum(j.job_execution_id)) [job_execution_id]
	--,j.job_name
	,j.job_version
	,isnull(j.step_id,0) [step_id]
	,j.step_name
	,j.is_active
	,j.lifecycle
	,j.create_time
	,j.start_time
	,j.end_time
	,case 
		when datediff(ss,j.start_time,j.end_time)&gt;86400 then cast(datediff(dd,j.start_time,j.end_time) as varchar(10))+'d ' else '' end
	+
		convert(varchar(20),dateadd(ss,datediff(ss,j.start_time,j.end_time),0),108) [duration]
	,j.current_attempts
	,j.last_message

	,m.target_group_name
	,j.target_type
	,j.target_resource_group_name
	,j.target_server_name
	,j.target_database_name
	,j.target_elastic_pool_name
	
from jobs.job_executions j
left join jobs.target_group_members m on j.target_id=m.target_id
where 
--and j.step_id is null 
 j.job_name = @jobname 
and j.start_time&gt;=convert(date,@date) and j.start_time&lt;dateadd(dd,1,convert(date,@date))
order by 
	j.job_execution_id,
	j.create_time asc ,
	j.end_time asc</CommandText>
      </Query>
      <Fields>
        <Field Name="job_execution_id">
          <DataField>job_execution_id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="job_version">
          <DataField>job_version</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="step_id">
          <DataField>step_id</DataField>
