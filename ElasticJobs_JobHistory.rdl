    <DataSet Name="dsHistoryPerDay">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@name">
            <Value>=Parameters!Job.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
set datefirst 1;
select 
--	dateadd(dd,-1*isnull(h.dw_int,wk.dd_int),isnull(h.[day],wk.[day])) [mon]
--	,dateadd(dd,(7-isnull(h.dw_int,wk.dd_int)),isnull(h.[day],wk.[day])) [sun]
convert(varchar(20),dateadd(dd,-1*isnull(h.dw_int,wk.dd_int),isnull(h.[day],wk.[day])),106) +' - '+convert(varchar(20),dateadd(dd,(7-isnull(h.dw_int,wk.dd_int)),isnull(h.[day],wk.[day])),106) [mon_sun]
	,convert(date,start_time) [day]
	,datename(dw,convert(date,start_time)) [dw]
	,datepart(dw,convert(date,start_time)) [dw_int]
	,datepart(iso_week,convert(date,start_time)) [week_num]
	,sum(iif(lifecycle='Succeeded',1,0)) [success]
	,sum(iif(lifecycle='Failed',1,0)) [fail]
	,sum(iif(lifecycle not in ('Failed','Succeeded'),1,0)) [other]
	,count(1) [ttl]
from jobs.job_executions
where step_id is null 
and job_name=@name
group by 
	convert(date,start_time)
*/

----------------------

set datefirst 1;
with dates as 
	(select 
			min(convert(date,start_time)) [start]
			,max(convert(date,start_time)) [end]
	from jobs.job_executions
	where job_name=@name --'Update Stats DWH100A'
	)  
,wk([day],dw,dd_int,n) as 
	(select 
		[start]
		,datename(dw,[start])
		,datepart(dw,[start])
		,0
	from dates 
	union all
	select
		dateadd(dd,1,[day])
		,datename(dw,dateadd(dd,1,[day]))
		,datepart(dw,dateadd(dd,1,[day]))
		,0
	from wk 
	where 
		dateadd(dd,1,[day])&lt;=(select [end] from dates)
	)  
,hist as (
	select 
		convert(date,start_time) [day]
		,datename(dw,convert(date,start_time)) [dw]
		,datepart(dw,convert(date,start_time)) [dw_int]
		,datepart(iso_week,convert(date,start_time)) [week_num]
		,sum(iif(lifecycle='Succeeded',1,0)) [success]
		,sum(iif(lifecycle='Failed',1,0)) [fail]
		,sum(iif(lifecycle not in ('Failed','Succeeded'),1,0)) [other]
		,count(1) [ttl]
	from jobs.job_executions
	where 
		step_id is null 
		and job_name=@name --'Update Stats DWH100A'
	group by 
		convert(date,start_time)
)
select 
	isnull(h.[day],wk.[day]) [day]
	,isnull(h.dw,wk.dw) [dw]
	,isnull(h.dw_int,wk.dd_int) [dw_int]
	,isnull(h.week_num,datepart(iso_week,wk.[day])) [week_num]
	,isnull(h.success,0) [success]
	,isnull(h.fail,0) [fail]
	,isnull(h.other,0) [other]
	,isnull(h.ttl,wk.n) [ttl]
	,dateadd(dd,-1*(isnull(h.dw_int,wk.dd_int)-1),isnull(h.[day],wk.[day])) [mon]
	,dateadd(dd,(7-isnull(h.dw_int,wk.dd_int)),isnull(h.[day],wk.[day])) [sun]
/*
,convert(varchar(20),dateadd(dd,-1*isnull(h.dw_int,wk.dd_int),isnull(h.[day],wk.[day])),106) +' - '+convert(varchar(20),dateadd(dd,(7-isnull(h.dw_int,wk.dd_int)),isnull(h.[day],wk.[day])),106) [monsun]
*/
from hist h
right join wk on wk.[day]=h.[day]</CommandText>
      </Query>
      <Fields>
        <Field Name="day">
          <DataField>day</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="dw">
          <DataField>dw</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="week_num">
          <DataField>week_num</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="success">
          <DataField>success</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="fail">
          <DataField>fail</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ttl">
          <DataField>ttl</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="other">
          <DataField>other</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="dw_int">
          <DataField>dw_int</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="mon">
          <DataField>mon</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="sun">
          <DataField>sun</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
