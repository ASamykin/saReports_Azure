    <DataSet Name="dsOperationStatus">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	start_time [start]
	,resource_type_desc [type]
	,major_resource_id [major]
	,minor_resource_id [minor]
	,operation 
	,state_desc [state]
	,percent_complete [perc]
	,error_code
