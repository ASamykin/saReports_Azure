    <DataSet Name="dsDatabases">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (
select 
	database_name [db]
	,max(end_time) [dt] 
from sys.resource_stats 
group by database_name
)
,sizes as (
select 
	s.database_name [db]
	,s.storage_in_megabytes [used]
	,s.allocated_storage_in_megabytes [alloc]
from sys.resource_stats s 
join data d on d.db=s.[database_name] and d.dt=s.end_time
)

select 
	d.name 
	,convert(smalldatetime,d.create_date) [created]
	,s.used
	,s.alloc
from sys.databases d 
join sizes s on d.name=s.db
where 
	d.database_id in (select database_id from sys.database_service_objectives where elastic_pool_name is not null)
order by 1 </CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="created">
          <DataField>created</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="used">
          <DataField>used</DataField>
