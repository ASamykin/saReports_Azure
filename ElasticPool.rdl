  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsWorkload">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select top 100 
	end_time [dt]
	,elastic_pool_name [pool]
	,avg_cpu_percent [cpu]
	,avg_data_io_percent [io]
	,avg_log_write_percent [log]
	,avg_storage_percent [storage]

	,elastic_pool_dtu_limit [dtulimit]
	,elastic_pool_storage_limit_mb [storlimit]
	
	,avg_instance_cpu_percent [instance_cpu]
	,avg_instance_memory_percent [instance_ram]
	,avg_allocated_storage_percent [instance_disk]

from sys.elastic_pool_resource_stats
order by start_time desc </CommandText>
      </Query>
    </DataSet>
  </DataSets>
