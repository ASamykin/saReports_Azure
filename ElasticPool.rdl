    <DataSet Name="dsPools">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
if object_id('tempdb..##log') is null 
	exec('create table ##log(i int identity,dt datetime default getdate(),[stage] varchar(50),[text] varchar(100))')

insert into ##log([stage],[text])
select 'dsPools','begin'
*/

;with 
data1 as (
	select top 1  
		elastic_pool_name [pool] 
		,edition 
	from sys.database_service_objectives 
	where elastic_pool_name is not null 
),
data2 as (
	select top 1 
		elastic_pool_name [pool]
		,elastic_pool_dtu_limit [dtu]
		,elastic_pool_storage_limit_mb [limit_mb]
		,ceiling(avg_storage_percent*elastic_pool_storage_limit_mb/100) [storage_used]
		,avg_storage_percent [used_perc]
	from sys.elastic_pool_resource_stats
	order by end_time desc 
)
select 
	d1.[pool]
	,d1.edition
	,d2.[dtu]
	,d2.[limit_mb]
	,d2.[storage_used]
	,d2.[used_perc]
from data1 d1 join data2 d2 on d1.[pool]=d2.[pool]

/*
insert into ##log([stage],[text])
select 'dsPools','end'
*/</CommandText>
      </Query>
      <Fields>
        <Field Name="pool">
          <DataField>pool</DataField>
          <rd:UserDefined>true</rd:UserDefined>
