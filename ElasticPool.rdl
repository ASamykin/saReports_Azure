    <DataSet Name="dsWorkload">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>

set datefirst 1;
with data as (
select 
	convert(time,convert(smalldatetime,end_time)) [dt]
	,elastic_pool_name [pool]
/*
	,avg(avg_cpu_percent) [cpu]
	,avg(avg_data_io_percent) [io]
	,avg(avg_log_write_percent) [log]
	,avg(avg_storage_percent) [storage]
	,avg(elastic_pool_storage_limit_mb*avg_storage_percent/100) [storageMB]
*/
	,max(avg_cpu_percent) [cpu]
	,max(avg_data_io_percent) [io]
	,max(avg_log_write_percent) [log]
	,max(avg_storage_percent) [storage]
	,max(elastic_pool_storage_limit_mb*avg_storage_percent/100) [storageMB]

	,max(elastic_pool_storage_limit_mb) [storage_max]

	,avg(avg_instance_cpu_percent) [instance_cpu]
	,avg(avg_instance_memory_percent) [instance_ram]
	,avg(avg_allocated_storage_percent) [instance_disk]

from sys.elastic_pool_resource_stats
where 
	datepart(dw,end_time)%datepart(dw,getdate())=0 /*current weekday*/
and end_time&lt;convert(date,getdate())
group by convert(time,convert(smalldatetime,end_time)), elastic_pool_name
)
, baseline as (
select 
	convert(smalldatetime,convert(varchar(20),getdate(),112)+' '+replace(convert(varchar(50),[dt]),'.0000000','')) [dt] 
	,[pool]
	,[cpu]
	,[io]
	,[log]
	,[storage]
	,[storageMB]
	,[instance_cpu]
	,[instance_ram]
	,[instance_disk]
from data
)
,today as (
select 
	convert(smalldatetime,end_time) [dt]
	--end_time [dt]
	,elastic_pool_name [pool]
	,max(avg_cpu_percent) [cpu]
	,max(avg_data_io_percent) [io]
	,max(avg_log_write_percent) [log]
	,max(avg_storage_percent) [storage]
	,max(elastic_pool_storage_limit_mb*avg_storage_percent/100) [storageMB]
	,max(elastic_pool_storage_limit_mb) [storage_max]

	,avg(elastic_pool_dtu_limit) [dtulimit]
	,avg(elastic_pool_storage_limit_mb) [storlimitMB]
	
	,avg(avg_instance_cpu_percent) [instance_cpu]
	,avg(avg_instance_memory_percent) [instance_ram]
	,avg(avg_allocated_storage_percent) [instance_disk]

from sys.elastic_pool_resource_stats
where end_time&gt;=convert(date,getdate())
group by convert(smalldatetime,end_time) , elastic_pool_name
)
select
	b.dt
	,isnull(t.[pool],b.[pool]) [pool]
	,t.[cpu]
	,t.[io]
	,t.[log]
	,t.[storage]
	,t.[storageMB]
	,t.[storage_max]
	,t.[instance_cpu]
	,t.[instance_ram]
	,t.[instance_disk]
	,b.cpu [bl_cpu]
	,b.[io] [bl_io]
	,b.[log] [bl_log]
from baseline b
left join today t on b.dt=t.dt</CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="pool">
          <DataField>pool</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cpu">
          <DataField>cpu</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="io">
          <DataField>io</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="log">
