          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="storage">
          <DataField>storage</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="dtulimit">
          <DataField>dtulimit</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="storlimitMB">
          <DataField>storlimitMB</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="instance_cpu">
          <DataField>instance_cpu</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="instance_ram">
          <DataField>instance_ram</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="instance_disk">
          <DataField>instance_disk</DataField>
