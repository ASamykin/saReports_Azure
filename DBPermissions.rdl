    <DataSet Name="dsRoleMembers">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@show">
            <Value>=Parameters!ShowRole.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @show tinyint = 1  /* 0 - all,  1 - effective*/

declare @cmd varchar(1024)

select @cmd='
with data as (
select 
	isnull(g.name,t.name) [group]
	,u.name [member]
	,u.create_date [created]
	,isnull(u.type_desc,''z'') [type]
	,u.modify_date [modified]
from sys.database_role_members rm
join sys.database_principals g on rm.role_principal_id=g.principal_id
join sys.database_principals u on rm.member_principal_id=u.principal_id
right join (select name from sys.database_principals where type=''R'') t on g.name=t.name
union all 

select 
	''public''
	,name
	,create_date
	,type_desc
	,modify_date
from sys.database_principals
where type_desc&lt;&gt;''DATABASE_ROLE''
and authentication_type_desc&lt;&gt;''NONE''
)
select 
	DENSE_RANK() OVER(ORDER BY [type],member) [n]
	,[group]
	,[member]
	,[created]
	,[modified]
	,[type]
from data '
+case 
	when @show=1 then 'where [member] is not null' 
	else ''
end

exec(@cmd)


</CommandText>
      </Query>
