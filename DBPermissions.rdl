  <DataSets>
    <DataSet Name="dsPermissions">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
object_name(major_id) [object]
,permission_name [permission]
, p.name [GrantedTo]
from sys.database_permissions dp
join sys.database_principals p on p.principal_id=dp.grantee_principal_id
order by 1
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="object">
          <DataField>object</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="permission">
          <DataField>permission</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="GrantedTo">
          <DataField>GrantedTo</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsRoleMembers">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	g.name [group]
	,u.name [member]
	,u.create_date [created]
from sys.database_role_members rm
join sys.database_principals g on rm.role_principal_id=g.principal_id
join sys.database_principals u on rm.member_principal_id=u.principal_id</CommandText>
      </Query>
      <Fields>
        <Field Name="group">
          <DataField>group</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="member">
