  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsFragment">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@tbl">
            <Value>=Parameters!Table.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select
	schema_name(t.schema_id) [schema]
	,t.name
	,p.partition_number

	,i.index_id
	,i.name [index]

	,stat.index_type_desc

	,p.[data_compression_desc]
	, ps.reserved_page_count
	, ps.reserved_page_count/128. [SizeMB]
	, ps.row_count

	,max(stat.index_depth) [index_depth]
	--,max(stat.avg_fragmentation_in_percent) [avg_fragmentation]
	,case 
		when p.[data_compression_desc]='COLUMNSTORE' then 
			(select avg(100*(ISNULL(deleted_rows,0))/total_rows) from sys.dm_db_column_store_row_group_physical_stats cs where cs.object_id=p.object_id and cs.index_id=i.index_id)
		else max(stat.avg_fragmentation_in_percent)
	end [avg_fragmentation]

	,sum(stat.page_count) [used_page_count]

	,STATS_DATE(p.object_id,i.index_id) [stats_updated]

from sys.partitions p
	join sys.tables t on t.object_id = p.object_id
	join sys.indexes i on p.object_id=i.object_id and p.index_id=i.index_id
	join sys.dm_db_partition_stats ps on (p.partition_id=ps.partition_id 
										and p.object_id=ps.object_id 
										and p.index_id=ps.index_id)
	cross apply sys.dm_db_index_physical_stats(db_id(),p.object_id,p.index_id,p.partition_number,default) stat
where p.object_id=object_id(@tbl)
group by 
	t.schema_id
	,p.object_id
	,t.name
	,p.partition_number
	,i.index_id
	,i.name
	,stat.index_type_desc
	,p.data_compression_desc
	,ps.reserved_page_count
	,ps.row_count
option (recompile)</CommandText>
      </Query>
      <Fields>
