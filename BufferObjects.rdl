  <DataSets>
    <DataSet Name="dsCacheObjects">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with obj as 
(select 
	object_id
    ,index_id
	,allocation_unit_id  
from sys.allocation_units au  
     join sys.partitions p on au.container_id = p.hobt_id and au.type in (1,3)
union all
select  
	object_id
	,index_id
	,allocation_unit_id  
from sys.allocation_units au
	join sys.partitions p on au.container_id = p.partition_id and au.type = 2  
) 
SELECT 
	schema_name(o.schema_id) [Schema]
    ,object_name(obj.object_id) [Object]
	,i.index_id   [IndexID]
	,i.name [IndexName]
	,convert(numeric(10,2),count(*)/128.) [MB]
	,convert(numeric(10,2),sum(convert(bigint,free_space_in_bytes))/1024./1024) [FreeMB]
from sys.dm_os_buffer_descriptors bd  
    join obj on bd.allocation_unit_id = obj.allocation_unit_id  
	join sys.objects o on obj.object_id=o.object_id
	join sys.indexes i on i.object_id=obj.object_id and i.index_id=obj.index_id
where database_id = DB_ID()  
group by o.schema_id,obj.object_id, i.index_id, i.name
order by 5 desc
option (recompile);</CommandText>
      </Query>
      <Fields>
        <Field Name="Schema">
          <DataField>Schema</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Object">
          <DataField>Object</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="IndexID">
