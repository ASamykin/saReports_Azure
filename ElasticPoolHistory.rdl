    <DataSet Name="dsDatabaseStats">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>if object_id('tempdb..##log') is null 
	exec('create table ##log(i int identity,dt datetime default getdate(),[stage] varchar(50),[text] varchar(100))')

insert into ##log([stage],[text])
select 'dsDatabaseStats','begin'


;with data as (
select 
	convert(smalldatetime,rs.end_time) [dt]
	,rs.database_name [db]
--	,d.database_id [dbid]


	,row_number() over (partition by convert(smalldatetime,rs.end_time) order by rs.avg_cpu_percent desc)  [n_cpu]
	,rs.avg_cpu_percent [cpu]

	,row_number() over (partition by convert(smalldatetime,rs.end_time) order by rs.avg_data_io_percent desc)  [n_io]
	,rs.avg_data_io_percent [io]

	,row_number() over (partition by convert(smalldatetime,rs.end_time) order by rs.avg_log_write_percent desc)  [n_log]
	,rs.avg_log_write_percent [log]

	,row_number() over (partition by convert(smalldatetime,rs.end_time) order by rs.max_session_percent desc)  [n_sess]
	,rs.max_session_percent [sess]

	,row_number() over (partition by convert(smalldatetime,rs.end_time) order by rs.max_worker_percent desc)  [n_worker]
	,rs.max_worker_percent [worker]
from sys.resource_stats rs 
--join sys.databases d on rs.database_name=d.name
)
, pools as 
	(
	select d.name [db]
		,dso.elastic_pool_name [pool]
	from sys.databases d 
	right join sys.database_service_objectives dso on d.database_id=dso.database_id
	)
select 
	 d.dt
	,d.db
	,d.n_cpu
	,d.cpu
	,d.n_io
	,d.[io]
	,d.n_log
	,d.[log]
	,d.n_sess
	,d.[sess]
	,d.n_worker
	,d.[worker]
	,p.pool
 from data d
 join pools p on p.db=d.db

insert into ##log([stage],[text])
select 'dsDatabaseStats','end'</CommandText>
