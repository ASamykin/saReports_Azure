  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>if object_id('tempdb..##log') is null 
	exec('create table ##log(i int identity,dt datetime default getdate(),[stage] varchar(50),[text] varchar(100))')

insert into ##log([stage],[text])
select 'dsCurrent','begin'

select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]

insert into ##log([stage],[text])
select 'dsCurrent','end'</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsWorkload">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>if object_id('tempdb..##log') is null 
	exec('create table ##log(i int identity,dt datetime default getdate(),[stage] varchar(50),[text] varchar(100))')

insert into ##log([stage],[text])
select 'dsWorkload','begin'

set datefirst 1

select 
	convert(smalldatetime,end_time) [dt]
	--end_time [dt]
	,elastic_pool_name [pool]
	,max(avg_cpu_percent) [cpu]
	,max(avg_data_io_percent) [io]
	,max(avg_log_write_percent) [log]
	,max(avg_storage_percent) [storage]
	,max(elastic_pool_storage_limit_mb*avg_storage_percent/100) [storageMB]

	,avg(elastic_pool_dtu_limit) [dtulimit]
	,avg(elastic_pool_storage_limit_mb) [storlimitMB]
	
	,avg(avg_instance_cpu_percent) [instance_cpu]
	,avg(avg_instance_memory_percent) [instance_ram]
	,avg(avg_allocated_storage_percent) [instance_disk]

	,datepart(dw,end_time) [dw]
	,datepart(wk,end_time) [wk]
	,left(convert(varchar(20),dateadd(wk,datepart(wk,end_time)-1,'20190101')-1,106),6)
		+  ' - ' +
	left(convert(varchar(20),dateadd(wk,datepart(wk,end_time)-1,'20190101')+5,106),6)
		[wk_desc]
into #elasticpool_data
from sys.elastic_pool_resource_stats
group by convert(smalldatetime,end_time) , elastic_pool_name , datepart(dw,end_time), datepart(wk,end_time)

select 
	[dt]
	,[pool]
	,[cpu]
	,[io]
	,[log]
	,[storage]
	,[storageMB]
	,[dtulimit]
	,[storlimitMB]
	,[instance_cpu]
	,[instance_ram]
	,[instance_disk]
	,[dw]
	,[wk]
	,[wk_desc]
from #elasticpool_data

insert into ##log([stage],[text])
select 'dsWorkload','end'</CommandText>
      </Query>
