  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info],
getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsChanges">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@name">
            <Value>=Parameters!Database.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with data as (
select 
	database_name [db]
	,end_time [dt]
	,sku+' '+convert(varchar(10),dtu_limit) [current] 
,lag(sku+' '+convert(varchar(10),dtu_limit),1,sku+' '+convert(varchar(10),dtu_limit)) over (partition by database_name order by end_time asc) [before]
from sys.resource_stats 
)
select 
	db
	,cast(dt as smalldatetime) [since]
	,[current]
	,[before]
	,row_number() over (partition by db order by dt) [n]
from data 
where [current]&lt;&gt;[before]
and db like @name
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="since">
          <DataField>since</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="current">
          <DataField>current</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="before">
          <DataField>before</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="n">
          <DataField>n</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDates">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	database_name [db]
	,min(start_time) [dt]
from sys.resource_stats
where database_name like '%%'
group by database_name</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
