  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsData">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!SPID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @spid int = 149
select 
	des.session_id  [spid]
	,des.login_name [loginname]
	,case when rtrim(ltrim(des.host_name))='' then con.client_net_address else des.host_name end [hostname]
	,des.program_name [programname]
	,des.host_process_id [PID]
	,des.status [status]
	,des.login_time [logintime]
	,st2.text [PrevQuery]

from sys.dm_exec_sessions  des
left join sys.dm_exec_connections con on des.session_id=con.session_id
outer APPLY sys.dm_exec_sql_text(con.most_recent_sql_handle) st2 

where 
	des.session_id=@id
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="spid">
          <DataField>spid</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
