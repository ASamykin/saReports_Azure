        <Field Name="id">
          <DataField>id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="latency">
          <DataField>latency</DataField>
          <rd:UserDefined>true</rd:UserDefined>
