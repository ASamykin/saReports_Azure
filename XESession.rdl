  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!SessionID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @id int = 65537;

with more as 
(
select 
	p.module_guid
	,p.name [package]
	,o.name [object]
	,o.description
from sys.dm_xe_packages p 
join sys.dm_xe_objects o on p.guid=o.package_guid
)
select 
	se.event_id [id]
	,se.name [event]
	,se.package
	,sa.name [action]
	,replace(m.description,'. ','.'+char(10)) [description]
from sys.database_event_session_events  se -- events to collect 
join sys.database_event_session_actions sa on se.event_session_id=sa.event_session_id  and se.event_id=sa.event_id
join more m on m.module_guid=se.module and m.object=se.name
where se.event_session_id=@id
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="event">
          <DataField>event</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="package">
          <DataField>package</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="action">
          <DataField>action</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsColumnDesc">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT p.name [package],
       o.name [event],
	   STRING_AGG('  '+c.name +' - '+isnull(c.description,'[no description]'), CHAR(10)) within group (order by c.name) [info]
FROM sys.dm_xe_objects o
JOIN sys.dm_xe_packages p ON o.package_guid = p.guid
JOIN sys.dm_xe_object_columns c ON o.name = c.object_name
WHERE 
	o.object_type = 'event'
	and c.column_type='data'
	--and p.name='sqlserver'
	--and o.name= 'sql_batch_completed'
group by p.name,o.name 
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="package">
          <DataField>package</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="event">
          <DataField>event</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsSession">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!SessionID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select 
	 s.event_session_id [id]
	,s.name
	,s.max_dispatch_latency [latency]
	,s.max_memory [maxmem]
	,s.startup_state [startup]
	,r.create_time [created]
	,r.total_buffer_size [ttlbuffer]
	,r.buffer_policy_desc [bufpolicy]
	,r.flag_desc [flag]
	,r.total_bytes_generated [ttlbytes]

	,(select 
			f.value
		from sys.database_event_session_fields f
		join sys.database_event_session_targets t 
			on f.event_session_id=t.event_session_id 
			and f.object_id=t.target_id 
			and f.name='max_file_size' 
		where f.event_session_id=s.event_session_id) [maxsize]

	,(select 
			f.value
		from sys.database_event_session_fields f
		join sys.database_event_session_targets t 
			on f.event_session_id=t.event_session_id 
			and f.object_id=t.target_id 
			and f.name='filename'
		where f.event_session_id=s.event_session_id) [file]

	,r.dropped_event_count [drpevents]
	,r.dropped_buffer_count [drpbuff]
	,r.largest_event_dropped_size [drplargest]

from sys.database_event_sessions s
left join sys.dm_xe_database_sessions r on s.name=r.name
where s.event_session_id=@id </CommandText>
      </Query>
