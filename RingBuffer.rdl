      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
	ring_buffer_type [type]
	,count(1) [n]
FROM sys.dm_os_ring_buffers
group by ring_buffer_type</CommandText>
      </Query>
      <Fields>
