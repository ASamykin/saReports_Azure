        <CommandText>/*
declare @type varchar(50) = 'RING_BUFFER_MEMORY_BROKER_CLERKS'--'RING_BUFFER_SECURITY_ERROR'
*/
declare @cmd varchar(4096)
declare @part1 varchar(255) 
declare @part2 varchar(4096)

set @part1='WITH hadr(ts, type, record) AS  
   (SELECT timestamp AS ts, ring_buffer_type AS type, CAST(record AS XML) AS record   
FROM sys.dm_os_ring_buffers   
WHERE ring_buffer_type = ''XXXXX'')  
, data as ('
set @cmd=replace(@part1,'XXXXX',@type)
--print @cmd

if @type='RING_BUFFER_SECURITY_ERROR'
	set @part2= '
SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''						,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(100)'') [Time]  
	,record.value(''(/Record/Error/SPID)[1]''				,''varchar(100)'') [SPID]  
	,record.value(''(/Record/Error/APIName)[1]''			,''varchar(100)'') [APIName]
	,record.value(''(/Record/Error/CallingAPIName)[1]''	,''varchar(100)'') [CallingAPIName]
	,record.value(''(/Record/Error/ErrorCode)[1]''		,''varchar(100)'') [ErrorCode]
	,record.value(''(/Record/Error/SQLErrorCode)[1]''		,''varchar(100)'') [SQL Error Code]  
	,record.value(''(/Record/Error/SQLErrorState)[1]''	,''varchar(100)'') [SQL Error State]  
FROM hadr  
) --select * from data order by 1 desc -- 128
select 
	ts,type,[Record ID] [rec],prop, val
from data   
unpivot (val for prop in ([Time],[SPID],[APIName],[CallingAPIName],[ErrorCode],[SQL Error Code],[SQL Error State])) un'


if @type='RING_BUFFER_MEMORY_BROKER_CLERKS'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]'',''bigint'') AS [Record id]
	,record.value(''(/Record/@time)[1]'',''varchar(100)'') AS [Time]  
	,record.value(''(/Record/MemoryBrokerClerk/Name)[1]'',					''varchar(100)'') AS [Name]  
	,record.value(''(/Record/MemoryBrokerClerk/TotalPages)[1]'',			''varchar(100)'') AS [TotalPages]
	,record.value(''(/Record/MemoryBrokerClerk/SimulatedPages)[1]'',		''varchar(100)'') AS [SimulatedPages]
	,record.value(''(/Record/MemoryBrokerClerk/InternalBenefit)[1]'',		''varchar(100)'') AS [InternalBenefit]
	,record.value(''(/Record/MemoryBrokerClerk/ExternalBenefit)[1]'',		''varchar(100)'') AS [ExternalBenefit]  
	,record.value(''(/Record/MemoryBrokerClerk/ValueOfMemory)[1]'',			''varchar(100)'') AS [ValueOfMemory]  
	,record.value(''(/Record/MemoryBrokerClerk/PeriodicFreedPages)[1]'',	''varchar(100)'') AS [PeriodicFreedPages]  
	,record.value(''(/Record/MemoryBrokerClerk/InternalFreedPages)[1]'',	''varchar(100)'') AS [InternalFreedPages]  
	FROM hadr  
) --select * from data order by 1 desc -- 128
select 
	ts,type,[Record ID] [rec],prop, val
from data   
unpivot (val for prop in ([Time],[Name],[TotalPages],[SimulatedPages],[InternalBenefit],[ExternalBenefit],[ValueOfMemory],[PeriodicFreedPages],[InternalFreedPages])) un'



if @part2=''
	set @cmd='select NULL [ts],NULL [type],NULL [rec], NULL [prop], NULL [val]'
else 
	set @cmd=@cmd+@part2


exec (@cmd)</CommandText>
      </Query>
