  <DataSets>
    <DataSet Name="dsSessions">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
select 
	des.session_id  [spid]
	,der.blocking_session_id [blockedby]
	,des.login_name [loginname]
	,case when rtrim(ltrim(des.host_name))='' then con.client_net_address else des.host_name end [hostname]
	,des.program_name [programname]
	,des.host_process_id [PID]
	,des.status [status]
	,des.login_time [logintime]
	,datediff(ss,'20180101',des.login_time) [logintimeid]
	,des.last_request_start_time [lastrequeststart]
	,des.last_request_end_time [lastrequestend]

	,des.cpu_time [cpu]
	,des.reads [reads]
	,des.writes [writes]
	,des.logical_reads [logicalreads]
	,des.total_elapsed_time [ttlelapsedtime]
	,des.transaction_isolation_level [tranlevel]
	,des.lock_timeout [locktimeout]
	--,des.memory_usage [memused]
	,des.transaction_isolation_level [isol]

	,der.wait_type [waittype]
	,der.wait_resource [waitresource]
	,der.wait_time [waittime]

	,der.status [status2]
	,der.command [command]

	,case when qp.query_plan is not null then 'View' else NULL end [plan]

	,der.sql_handle [sqlhandle]
	,der.query_hash [queryhash]
	,der.query_plan_hash [qplanhash]
	,der.plan_handle [planhandle]
	,der.scheduler_id [scheduler]

	,st.text
	,st.encrypted

	,convert(varchar(20),dateadd(ss,datediff(ss,der.start_time,getdate()),0),108) [active]

	,wg.name + ' '+quotename(wg.importance) [group]
	,des.row_count [rows]

	,st2.text [PrevQuery]

from sys.dm_exec_sessions  des
left join sys.dm_exec_connections con on des.session_id=con.session_id
left join sys.dm_exec_requests der on des.session_id=der.session_id
left join sys.resource_governor_workload_groups wg on des.group_id=wg.group_id
outer APPLY sys.dm_exec_sql_text(der.sql_handle) st
outer APPLY sys.dm_exec_sql_text(con.most_recent_sql_handle) st2 
outer APPLY sys.dm_exec_query_plan(der.plan_handle) qp

where des.is_user_process=1
order by des.status, des.session_id
option (recompile)</CommandText>
