    <DataSet Name="dsJobs">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@grp">
            <Value>=Parameters!Group.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @grp varchar(50) = 'DWH100A'
exec sp_executesql N'select 
    j.name [job_name]
    ,jv.job_version_number [job_version]
    ,js.step_id
    ,js.step_name
    ,t.target_group_name  
from [jobs_internal].visible_jobs j  
inner join [jobs_internal].job_versions jv					on j.job_id = jv.job_id  
inner join [jobs_internal].current_job_version_numbers cjvn on jv.job_id = cjvn.job_id and jv.job_version_number = cjvn.current_job_version_number
inner join [jobs_internal].jobsteps js						on jv.job_id = JS.job_id and jv.job_version_number = js.job_version_number  
inner join [jobs_internal].jobstep_data jsd					on js.jobstep_data_id = jsd.jobstep_data_id  
left outer join [jobs_internal].visible_target_groups t     on jsd.target_id = t.target_id  
where 
	t.target_group_name = @group',N'@group varchar(50)',@group=@grp</CommandText>
      </Query>
      <Fields>
        <Field Name="job_name">
          <DataField>job_name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="job_version">
          <DataField>job_version</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="step_id">
          <DataField>step_id</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="step_name">
          <DataField>step_name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="target_group_name">
          <DataField>target_group_name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
