    <DataSet Name="dsWorkloadPerc">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dt">
            <Value>=Parameters!Date.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
declare @db nvarchar(128) = N'EDW'
declare @dt datetime = getdate()
*/
exec sp_executesql N'
select 
convert(date,@date),
	sum(avg_cpu_percent)/case when sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent)=0 then 1 else sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent) end [Cpu]
	,sum(avg_data_io_percent)/case when sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent)=0 then 1 else sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent) end [DataIO]
	,sum(avg_log_write_percent)/case when sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent)=0 then 1 else sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent) end [LogWr]
from sys.resource_stats rs
where 
	database_name=@database
	and  end_time between convert(date,@date) and dateadd(dd,1,convert(date,@date))
group by convert(date,end_time)
option (recompile)',@params=N'@database nvarchar(128), @date datetime',@database=@db,@date=@dt</CommandText>
      </Query>
      <Fields>
        <Field Name="Cpu">
          <DataField>Cpu</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="DataIO">
          <DataField>DataIO</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="LogWr">
          <DataField>LogWr</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsGrowthDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt">
            <Value>=Parameters!Date.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd nvarchar(2048)
--declare @db nvarchar(128) = 'EDW'--'OTR_OTH'
--declare @dt date = '20190528'--getdate()

set  @cmd=N'
with data as (
select 
	database_name
	,end_time
	,storage_in_megabytes  [used_now]
	,lag(storage_in_megabytes,1,storage_in_megabytes) over (partition by database_name order by end_time )[used_before]
	,allocated_storage_in_megabytes [alloc_now]
	,lag(allocated_storage_in_megabytes,1,allocated_storage_in_megabytes) over (partition by database_name order by end_time )[alloc_before]
from sys.resource_stats 
where 
database_name=@dbname and
end_time between @date and dateadd(dd,1,@date)
)
select 
	convert(smalldatetime,end_time) [dt]
	,database_name [db]
	,used_before
	,used_now
	,used_now-used_before [used_growth]
	,alloc_before
	,alloc_now
	,alloc_now-alloc_before [alloc_growth]
from data 
where 
	used_before&lt;&gt;used_now 
	or 
	alloc_before&lt;&gt;alloc_now
order by 1 desc'
exec sp_executesql @cmd,N'@dbname nvarchar(128), @date date' ,@dbname=@db,@date=@dt </CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="used_before">
          <DataField>used_before</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="used_now">
          <DataField>used_now</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="used_growth">
          <DataField>used_growth</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="alloc_before">
          <DataField>alloc_before</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="alloc_now">
          <DataField>alloc_now</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="alloc_growth">
          <DataField>alloc_growth</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsOperationStatus">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
select 
	convert(varchar(256),major_resource_id) [resource]
	,start_time
	,operation
	,percent_complete [percent]
	,state_desc [Status]
	,resource_type_desc+
				+quotename(convert(varchar(255),major_resource_id))
				+' operation '+operation
				+' is '+state_desc
				+' ('+convert(varchar(10),percent_complete)
				+'%), started at '+convert(varchar(20),start_time,106)+' '+convert(varchar(20),start_time,108) 
				+ ', '+case when state_desc='IN_PROGRESS' then 'running for ' else 'finished in ' end + convert(varchar(20),dateadd(ss,datediff(ss,start_time,last_modify_time),0),108)
				 [desc]
	,error_code
	,error_desc
from sys.dm_operation_status
order by 2 desc
*/
with data as (
select 
	database_name [db]
	,end_time [dt]
	,sku+' '+convert(varchar(10),dtu_limit) [service] 
	,lag(sku+' '+convert(varchar(10),dtu_limit),1,sku+' '+convert(varchar(10),dtu_limit)) over (partition by database_name order by end_time desc) [new]
from sys.resource_stats 
where database_name=@db
)
select 
		db [resource]
		,convert(smalldatetime,dt) [start_time]
		,'Tier changed' [operation]
		,100 [percent]
		,'completed' [Status]
		,'Service tier change : '+[service]+' -&gt; '+[new] [desc]
		,null [error_code]
		,null [error_desc]
from data 
where [service]&lt;&gt;[new]

union all 

select 
	convert(varchar(256),major_resource_id) [resource]
	,start_time
	,operation
	,percent_complete [percent]
	,state_desc [Status]
	,resource_type_desc+
				+quotename(convert(varchar(255),major_resource_id))
				+' operation '+operation
				+' is '+state_desc
				+' ('+convert(varchar(10),percent_complete)
				+'%), started at '+convert(varchar(20),start_time,106)+' '+convert(varchar(20),start_time,108) 
				+ ', '+case when state_desc='IN_PROGRESS' then 'running for ' else 'finished in ' end + convert(varchar(20),dateadd(ss,datediff(ss,start_time,last_modify_time),0),108)
				 [desc]
	,error_code
	,error_desc
from sys.dm_operation_status
order by 2 desc
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="resource">
          <DataField>resource</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="start_time">
          <DataField>start_time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="operation">
          <DataField>operation</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="percent">
          <DataField>percent</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="error_code">
          <DataField>error_code</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="error_desc">
          <DataField>error_desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
