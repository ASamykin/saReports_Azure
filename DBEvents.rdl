    <DataSet Name="dsEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
with dates as 
(
select convert(smalldatetime,convert(date,getdate())) [dt]
	,'' [type]
	,0 [count]

union all
select dateadd(mi,5,[dt])
	,'' [type]
	,0 [count]
from dates 
where dt&lt;convert(date,dateadd(dd,1,getdate()))
)
,vals  as 
(
select 
	convert(smalldatetime,start_time) [time]
	,event_category+': '+event_type [type]
	,event_count [count]

from sys.event_log
where start_time&gt;=convert(date,getdate())
and database_name=@db
)
select 
	isnull(v.time,d.dt) [time]
	,isnull(v.type,d.type) [type]
	,isnull(v.count,d.count) [count]
from vals v
right outer join dates d on v.time=d.dt
option (maxrecursion 0,recompile)
*/
select 
	convert(smalldatetime,start_time) [time]
	,event_category+': '+event_type [type]
	,event_count [count]
	,description
from sys.event_log
where start_time&gt;=convert(date,getdate())
and database_name=@db
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
