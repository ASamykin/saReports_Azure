    <DataSet Name="dsEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dt">
            <Value>=Parameters!Date.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>exec sp_executesql N'
select 
	convert(smalldatetime,start_time) [time]
	,event_category+'': ''+event_type [type]
	,event_type [type2]
	,event_count [count]
	,description
from sys.event_log
where 
(start_time between convert(date,@date) and dateadd(dd,1,convert(date,@date))
)
and database_name=@database
/*option (recompile)*/',@params=N'@database nvarchar(128),@date datetime',@database=@db,@date=@dt</CommandText>
      </Query>
      <Fields>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="type2">
          <DataField>type2</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
