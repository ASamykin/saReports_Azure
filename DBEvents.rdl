    <DataSet Name="dsWorkload">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DBName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set datefirst 1
--declare @db nvarchar(256) =N'master'
select 
	convert(smalldatetime,end_time) [end]
	,datepart(dw,end_time) [dw]
	,datepart(wk,end_time) [wk]
	,left(convert(varchar(20),dateadd(wk,datepart(wk,end_time)-1,'20190101')-1,106),6)
		+  ' - ' +
	left(convert(varchar(20),dateadd(wk,datepart(wk,end_time)-1,'20190101')+5,106),6)
		[wk_desc]
	,database_name [db]
	,sku
	,dtu_limit [DTULimit]
	,storage_in_megabytes [used]
	,allocated_storage_in_megabytes [alloc]
	,storage_in_megabytes/allocated_storage_in_megabytes*100 [storagealloc]
	,avg_cpu_percent [Cpu]
	,avg_data_io_percent [DataIO]
	,avg_log_write_percent [LogWr]
	,max_worker_percent [MaxWorker]
	,max_session_percent [MaxSess]
   ,(select MAX(Used) 
		from (values (avg_cpu_percent)
					,(avg_data_io_percent)
					,(avg_log_write_percent)
					,(max_worker_percent)
					,(max_session_percent)
			) [MaxUsed](Used)
	) [MaxUsed]
from sys.resource_stats rs
where 
	database_name=@db
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="end">
          <DataField>end</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
