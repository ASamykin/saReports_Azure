    <DataSet Name="dsQueriesPlans">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (
select 
	query_id
	,convert(int,is_forced_plan) [is_forced_plan]
	,count(1) n
	
from sys.query_store_plan
group by query_id,is_forced_plan
)
select 
	count(1) [queries]
	,n [cnt] -- amount of plans
	,sum(is_forced_plan) [forced]
	,STRING_AGG(convert(varchar(10),query_id),',') within group (order by query_id) [ids]
from data 
where n&gt;1
group by n
union all 
select 
	count(1) [queries]
	,n [cnt]  -- amount of plans
	,sum(is_forced_plan) [forced]
	,'' [ids]
from data 
where n=1
group by n</CommandText>
      </Query>
      <Fields>
        <Field Name="queries">
          <DataField>queries</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cnt">
          <DataField>cnt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ids">
          <DataField>ids</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="forced">
          <DataField>forced</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsQueriesToday">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
SELECT rsi.end_time
	,count(DISTINCT p.query_id) [n]
FROM sys.query_store_plan p
INNER JOIN sys.query_store_runtime_stats rs ON rs.plan_id = p.plan_id
INNER JOIN sys.query_store_runtime_stats_interval rsi ON rs.runtime_stats_interval_id = rsi.runtime_stats_interval_id
WHERE rsi.end_time &gt;= convert(DATE, getdate())
GROUP BY rsi.end_time
*/
WITH data
AS (
	SELECT DISTINCT rsi.end_time
		,p.query_id [query_id]
		,iif(q.is_internal_query ^ q.is_clouddb_internal_query = 1, 1, 0) [is_system]
		,iif(q.is_internal_query ^ q.is_clouddb_internal_query = 1, 0, 1) [is_user]
	FROM sys.query_store_plan p
	JOIN sys.query_store_query q ON p.query_id = q.query_id
	INNER JOIN sys.query_store_runtime_stats rs ON rs.plan_id = p.plan_id
	INNER JOIN sys.query_store_runtime_stats_interval rsi ON rs.runtime_stats_interval_id = rsi.runtime_stats_interval_id
	WHERE rsi.end_time &gt;= convert(DATE, getdate())
	)
SELECT [end_time]
	,count(1) [n]
	,sum([is_system]) [ttl_system]
	,sum([is_user]) [ttl_user]
FROM data
GROUP BY [end_time]</CommandText>
      </Query>
      <Fields>
        <Field Name="end_time">
          <DataField>end_time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="n">
          <DataField>n</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ttl_system">
          <DataField>ttl_system</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ttl_user">
          <DataField>ttl_user</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsWaitsToday">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (
select 
	convert(smalldatetime,rsi.end_time) [end_time]
	,ws.wait_category_desc [wait_type]
	,sum(ws.total_query_wait_time_ms) [total_wait_ms]
from sys.query_store_wait_stats ws
join sys.query_store_runtime_stats_interval rsi on ws.runtime_stats_interval_id=rsi.runtime_stats_interval_id
where rsi.end_time&gt;=convert(date,getdate())
group by 
	rsi.end_time
	,ws.wait_category_desc
)

select 
	[end_time]
	,[wait_type]
	,[total_wait_ms]
	,100. *  [total_wait_ms] / SUM ([total_wait_ms]) over (partition by [end_time] )  [percentage_current]
from data
</CommandText>
      </Query>
      <Fields>
        <Field Name="end_time">
          <DataField>end_time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="wait_type">
          <DataField>wait_type</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="total_wait_ms">
          <DataField>total_wait_ms</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="percentage_current">
          <DataField>percentage_current</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsWaitsLegend">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*  source:
https://docs.microsoft.com/en-us/sql/relational-databases/system-catalog-views/sys-query-store-wait-stats-transact-sql
*/

with data as 
(
	select 'Unknown'[Category], 'Unknown' [Waits]  union all
	select 'CPU'[Category], 'SOS_SCHEDULER_YIELD' [Waits]  union all
	select 'Worker Thread'[Category], 'THREADPOOL' [Waits]  union all
	select 'Lock'[Category], 'LCK_M_%' [Waits]  union all
	select 'Latch'[Category], 'LATCH_%' [Waits]  union all
	select 'Buffer Latch'[Category], 'PAGELATCH_%' [Waits]  union all
	select 'Buffer IO'[Category], 'PAGEIOLATCH_%' [Waits]  union all
	select 'Compilation*'[Category], 'RESOURCE_SEMAPHORE_QUERY_COMPILE' [Waits]  union all
	select 'SQL CLR'[Category], 'CLR%, SQLCLR%' [Waits]  union all
	select 'Mirroring'[Category], 'DBMIRROR%' [Waits]  union all
	select 'Transaction'[Category], 'XACT%, DTC%, TRAN_MARKLATCH_%, MSQL_XACT_%, TRANSACTION_MUTEX' [Waits]  union all
	select 'Idle'[Category], 'SLEEP_%, LAZYWRITER_SLEEP, SQLTRACE_BUFFER_FLUSH, SQLTRACE_INCREMENTAL_FLUSH_SLEEP, SQLTRACE_WAIT_ENTRIES, FT_IFTS_SCHEDULER_IDLE_WAIT, XE_DISPATCHER_WAIT, REQUEST_FOR_DEADLOCK_SEARCH, LOGMGR_QUEUE, ONDEMAND_TASK_QUEUE, CHECKPOINT_QUEUE, XE_TIMER_EVENT' [Waits]  union all
	select 'Preemptive'[Category], 'PREEMPTIVE_%' [Waits]  union all
	select 'Service Broker'[Category], 'BROKER_% (but not BROKER_RECEIVE_WAITFOR)' [Waits]  union all
	select 'Tran Log IO'[Category], 'LOGMGR, LOGBUFFER, LOGMGR_RESERVE_APPEND, LOGMGR_FLUSH, LOGMGR_PMM_LOG, CHKPT, WRITELOG' [Waits]  union all
	select 'Network IO'[Category], 'ASYNC_NETWORK_IO, NET_WAITFOR_PACKET, PROXY_NETWORK_IO, EXTERNAL_SCRIPT_NETWORK_IOF' [Waits]  union all
	select 'Parallelism'[Category], 'CXPACKET, EXCHANGE' [Waits]  union all
	select 'Memory'[Category], 'RESOURCE_SEMAPHORE, CMEMTHREAD, CMEMPARTITIONED, EE_PMOLOCK, MEMORY_ALLOCATION_EXT, RESERVED_MEMORY_ALLOCATION_EXT, MEMORY_GRANT_UPDATE' [Waits]  union all
	select 'User Wait'[Category], 'WAITFOR, WAIT_FOR_RESULTS, BROKER_RECEIVE_WAITFOR' [Waits]  union all
	select 'Tracing'[Category], 'TRACEWRITE, SQLTRACE_LOCK, SQLTRACE_FILE_BUFFER, SQLTRACE_FILE_WRITE_IO_COMPLETION, SQLTRACE_FILE_READ_IO_COMPLETION, SQLTRACE_PENDING_BUFFER_WRITERS, SQLTRACE_SHUTDOWN, QUERY_TRACEOUT, TRACE_EVTNOTIFF' [Waits]  union all
	select 'Full Text Search'[Category], 'FT_RESTART_CRAWL, FULLTEXT GATHERER, MSSEARCH, FT_METADATA_MUTEX, FT_IFTSHC_MUTEX, FT_IFTSISM_MUTEX, FT_IFTS_RWLOCK, FT_COMPROWSET_RWLOCK, FT_MASTER_MERGE, FT_PROPERTYLIST_CACHE, FT_MASTER_MERGE_COORDINATOR, PWAIT_RESOURCE_SEMAPHORE_FT_PARALLEL_QUERY_SYNC' [Waits]  union all
	select 'Other Disk IO'[Category], 'ASYNC_IO_COMPLETION, IO_COMPLETION, BACKUPIO, WRITE_COMPLETION, IO_QUEUE_LIMIT, IO_RETRY' [Waits]  union all
	select 'Replication'[Category], 'SE_REPL_%, REPL_%, HADR_% (but not HADR_THROTTLE_LOG_RATE_GOVERNOR), PWAIT_HADR_%, REPLICA_WRITES, FCB_REPLICA_WRITE, FCB_REPLICA_READ, PWAIT_HADRSIM' [Waits]  union all
	select 'Log Rate Governor'[Category], 'LOG_RATE_GOVERNOR, POOL_LOG_RATE_GOVERNOR, HADR_THROTTLE_LOG_RATE_GOVERNOR, INSTANCE_LOG_RATE_GOVERNOR' [Waits] 
)
select 
   Category, Waits
from data </CommandText>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Waits">
          <DataField>Waits</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
