    <DataSet Name="dsConfiguration">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
with data(n,txt) as 
(
select 0,'n/a' union all
select 1,'Database is in read-only mode.' union all
select 2,'Database is in single-user mode.' union all
select 4,'Database is in emergency mode.' union all
select 8,'Database is secondary replica.' union all
select 65536,'QS size limit is reached.' union all
select 131072,'The number of different statements has reached the internal memory limit.' union all
select 262144,'Size of in-memory items waiting to be persisted on disk has reached the internal memory limit.' union all
select 524288,'Database has reached disk size limit.'
) 
select 
	 so.actual_state_desc	[state_actual]
	,so.desired_state_desc [state_desired]
	,so.query_capture_mode_desc [capture_mode]
	,so.size_based_cleanup_mode_desc	[size_based_cleanup]
	,so.wait_stats_capture_mode_desc	[wait_stats_capture]
	,so.readonly_reason
	,d.txt

	,so.current_storage_size_mb [current_mb]
	,so.max_storage_size_mb	[max_mb]

	,so.flush_interval_seconds	[flush_sec]
	,so.interval_length_minutes	[interval_min]

	,so.stale_query_threshold_days [stale_threshold]
	,so.max_plans_per_query	[max_plans]
from sys.database_query_store_options so
join data d on so.readonly_reason=d.n</CommandText>
      </Query>
