          <QueryParameter Name="@srv">
            <Value>=Parameters!Server.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @server varchar(128) = 'sw20dwh200a'
exec sp_executesql N'select database_name [db] from dbo.raw_resource_stats where srv=@server group by database_name',N'@server varchar(128)',@server=@srv</CommandText>
      </Query>
      <Fields>
