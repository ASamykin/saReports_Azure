  <DataSets>
    <DataSet Name="dsPlan">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!SPID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @cmd nvarchar(4000)
set @cmd='
select 
     db_name(qp.dbid) [db]
,object_name(qp.objectid) [object]
,qp.encrypted
,qp.query_plan [plan]
,st.text
,convert(smalldatetime,der.start_time) [start]
,case when datediff(ss,der.start_time,getdate())&gt;86400 then convert(varchar(20),datediff(dd,der.start_time,getdate()))+''d '' else '''' end +
	convert(varchar(20),dateadd(ss,datediff(ss,der.start_time,getdate()),0),108) [duration]
,user_name(der.user_id) [username]
,der.session_id [spid]

,der.cpu_time
,der.reads
,der.logical_reads
,der.writes
,der.total_elapsed_time

,der.blocking_session_id  [blockedby]
,der.dop
from 
sys.dm_exec_requests der 
cross apply sys.dm_exec_query_plan(der.plan_handle) qp
cross apply sys.dm_exec_sql_text(der.sql_handle) st
where der.session_id=@spid option (recompile)'
exec sp_executesql @cmd,N'@spid int',@spid=@id</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="object">
          <DataField>object</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="encrypted">
          <DataField>encrypted</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="plan">
          <DataField>plan</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="text">
          <DataField>text</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="start">
          <DataField>start</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="duration">
          <DataField>duration</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="username">
          <DataField>username</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="spid">
          <DataField>spid</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="cpu_time">
          <DataField>cpu_time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
