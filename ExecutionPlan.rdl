    <DataSet Name="dsSessionSets">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@session">
            <Value>=Parameters!SPID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with data as (
select 
	convert(varchar(50),language)  [language]
	,convert(varchar(50),date_format) [date_format]
	,convert(varchar(50),date_first) [date_first]
	,convert(varchar(50),quoted_identifier) [quoted_identifier]
	,convert(varchar(50),arithabort) [arithabort]
	,convert(varchar(50),ansi_null_dflt_on) [ansi_null_dflt_on]
	,convert(varchar(50),ansi_defaults) [ansi_defaults]
	,convert(varchar(50),ansi_warnings) [ansi_warnings]
	,convert(varchar(50),ansi_padding) [ansi_padding]
	,convert(varchar(50),ansi_nulls) [ansi_nulls]
	,convert(varchar(50),concat_null_yields_null) [concat_null_yields_null]
	,convert(varchar(50),transaction_isolation_level) [transaction_isolation_level]
	,convert(varchar(50),lock_timeout) [lock_timeout]
	,convert(varchar(50),deadlock_priority) [deadlock_priority]
from sys.dm_exec_requests
 where session_id=@session
 )
 select
   par
   ,val
from data
unpivot 
(val for par in([language],[date_format],[date_first],[quoted_identifier],[arithabort],[ansi_null_dflt_on],[ansi_defaults],[ansi_warnings],[ansi_padding],[ansi_nulls],[concat_null_yields_null],[transaction_isolation_level],[lock_timeout],[deadlock_priority])) p
order by par
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="par">
          <DataField>par</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCurrent">
