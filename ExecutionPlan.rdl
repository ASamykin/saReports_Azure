        <Field Name="reads">
          <DataField>reads</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="logical_reads">
          <DataField>logical_reads</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="writes">
          <DataField>writes</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="total_elapsed_time">
          <DataField>total_elapsed_time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
