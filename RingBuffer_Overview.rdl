  <DataSets>
    <DataSet Name="dsCurrent">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select
  quotename(db_name())+' @ '+convert(varchar(50),@@servername) [info]
,getdate() [time]</CommandText>
      </Query>
      <Fields>
        <Field Name="info">
          <DataField>info</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="time">
          <DataField>time</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsEventsGrouped">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt1">
            <Value>=Parameters!parDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with data as (  
/*RING_BUFFER_CONNECTIVITY: get one pair time (timestamp) + RecordTime (datetime) to use it as a base to 
calculate date and time for all other events as  they only have "timestamp" column (bigint). 
*/
select top 1
	record.value('(Record/@time)[1]', 'bigint') [time_int],
	record.value('(Record/ConnectivityTraceRecord/RecordTime)[1]', 'datetime') [time_dt]
from
		( select cast(record as xml) as record
			from sys.dm_os_ring_buffers
			where ring_buffer_type = 'RING_BUFFER_CONNECTIVITY'
		) as tab
	)
, data2 as 
(
select 
	dateadd(ss,(rb.timestamp-d.time_int)/1000,d.time_dt) [dt]
	,rb.ring_buffer_type [event]
from data d
cross join sys.dm_os_ring_buffers rb
)
select 
	convert(smalldatetime,dateadd(hh,datediff(hh,0,[dt]),0)) [dt_hh]
	,[event]
	,count(1) [nn_hh]
from data2
--where [dt]&gt;=convert(date,getdate())
where [dt]&gt;=@dt1 and [dt]&lt;dateadd(dd,1,@dt1)
group by dateadd(hh,datediff(hh,0,[dt]),0) , [event]
order by 1 desc </CommandText>
      </Query>
      <Fields>
        <Field Name="dt_hh">
          <DataField>dt_hh</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="event">
          <DataField>event</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="nn_hh">
          <DataField>nn_hh</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsEventsCalendar">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>set datefirst 1;
with data as (  
/*RING_BUFFER_CONNECTIVITY: get one pair time (timestamp) + RecordTime (datetime) to use it as a base to 
calculate date and time for all other events as  they only have "timestamp" column (bigint). 
*/
select top 1
	record.value('(Record/@time)[1]', 'bigint') [time_int],
	record.value('(Record/ConnectivityTraceRecord/RecordTime)[1]', 'datetime') [time_dt]
from
		( select cast(record as xml) as record
			from sys.dm_os_ring_buffers
			where ring_buffer_type = 'RING_BUFFER_CONNECTIVITY'
		) as tab
	)
, data2 as 
(
select 
	dateadd(ss,(rb.timestamp-d.time_int)/1000,d.time_dt) [dt]
	,rb.ring_buffer_type [event]
from data d
cross join sys.dm_os_ring_buffers rb
)
,final as 
(
select 
	convert(date,dateadd(dd,datediff(dd,0,[dt]),0)) [dt_hh]
	,[event]
	,count(1) [nn_hh]
from data2
group by dateadd(dd,datediff(dd,0,[dt]),0), [event]
)
select 
	[dt_hh] [dt]
	,datepart(dw,[dt_hh]) [weekday_int]
	,datename(weekday,[dt_hh]) [wd]
	,datepart(wk,[dt_hh]) [wk]
	,left(convert(varchar(20),dateadd(wk,datepart(wk,[dt_hh])-1,'20190101')-1,106),6)
		+  ' - ' +
	left(convert(varchar(20),dateadd(wk,datepart(wk,[dt_hh])-1,'20190101')+5,106),6)
		[week_desc]
,[event] 
,[nn_hh]
from final 
--order by 1 desc </CommandText>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="weekday_int">
          <DataField>weekday_int</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="wd">
          <DataField>wd</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="wk">
          <DataField>wk</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="week_desc">
          <DataField>week_desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="event">
          <DataField>event</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="nn_hh">
          <DataField>nn_hh</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsEventsDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@type">
            <Value>=Parameters!parEvent.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@dt">
            <Value>=Parameters!parDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
declare @type varchar(50) = 'RING_BUFFER_EXCEPTION'--'RING_BUFFER_XE_LOG'--'RING_BUFFER_AUTO_ELEVATE_PFS_UP_LATCH'--'RING_BUFFER_MEMORY_BROKER_CLERKS'--'RING_BUFFER_SECURITY_ERROR'
declare @dt date = getdate()
*/
declare @cmd varchar(8000)
declare @part1 varchar(8000) 
declare @part2 varchar(8000)

set @part1='
with dt as 
	(
	select 
		record.value(''(Record/@time)[1]'', ''bigint'') [time_int],
		record.value(''(Record/ConnectivityTraceRecord/RecordTime)[1]'', ''datetime'') [time_dt]
	from
		( select top 1 cast(record as xml) as record
			from sys.dm_os_ring_buffers
			where ring_buffer_type = ''RING_BUFFER_CONNECTIVITY''
		) as tab
	)
,bt(ts, type, record) as  
   (select  dateadd(ss,(rb.timestamp-d.time_int)/1000,d.time_dt) [ts]
			,rb.ring_buffer_type [type]
			,convert(XML,rb.record) [record]
	from sys.dm_os_ring_buffers rb
	cross join dt d
	where ring_buffer_type = ''XXXXX''
	and dateadd(ss,(rb.timestamp-d.time_int)/1000,d.time_dt) &gt;= @date 
	and dateadd(ss,(rb.timestamp-d.time_int)/1000,d.time_dt) &lt;  dateadd(dd,1,@date)
	)  
,data as ('
set @cmd=replace(replace(@part1,'XXXXX',@type),'@date',''''+convert(varchar(10),@dt,112)+'''')

if @type='RING_BUFFER_SECURITY_ERROR'
	set @part2= '
SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''						,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(100)'') [Time]  
	,record.value(''(/Record/Error/SPID)[1]''				,''varchar(100)'') [SPID]  
	,record.value(''(/Record/Error/APIName)[1]''			,''varchar(100)'') [APIName]
	,record.value(''(/Record/Error/CallingAPIName)[1]''	,''varchar(100)'') [CallingAPIName]
	,record.value(''(/Record/Error/ErrorCode)[1]''		,''varchar(100)'') [ErrorCode]
	,record.value(''(/Record/Error/SQLErrorCode)[1]''		,''varchar(100)'') [SQL Error Code]  
	,record.value(''(/Record/Error/SQLErrorState)[1]''	,''varchar(100)'') [SQL Error State]  
FROM bt  
)
select [nnn],
	ts,type,[Record ID] [rec],prop, val
from data   
unpivot (val for prop in ([Time],[SPID],[APIName],[CallingAPIName],[ErrorCode],[SQL Error Code],[SQL Error State])) un'

if @type='RING_BUFFER_MEMORY_BROKER_CLERKS'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]'',''bigint'') AS [Record id]
	,record.value(''(/Record/@time)[1]'',''varchar(100)'') AS [Time]  
	,record.value(''(/Record/MemoryBrokerClerk/Name)[1]'',					''varchar(100)'') AS [Name]  
	,record.value(''(/Record/MemoryBrokerClerk/TotalPages)[1]'',			''varchar(100)'') AS [TotalPages]
	,record.value(''(/Record/MemoryBrokerClerk/SimulatedPages)[1]'',		''varchar(100)'') AS [SimulatedPages]
	,record.value(''(/Record/MemoryBrokerClerk/InternalBenefit)[1]'',		''varchar(100)'') AS [InternalBenefit]
	,record.value(''(/Record/MemoryBrokerClerk/ExternalBenefit)[1]'',		''varchar(100)'') AS [ExternalBenefit]  
	,record.value(''(/Record/MemoryBrokerClerk/ValueOfMemory)[1]'',			''varchar(100)'') AS [ValueOfMemory]  
	,record.value(''(/Record/MemoryBrokerClerk/PeriodicFreedPages)[1]'',	''varchar(100)'') AS [PeriodicFreedPages]  
	,record.value(''(/Record/MemoryBrokerClerk/InternalFreedPages)[1]'',	''varchar(100)'') AS [InternalFreedPages]  
	FROM bt 
)
select [nnn],
	ts,type,[Record ID] [rec],prop, val
from data   
unpivot (val for prop in ([Time],[Name],[TotalPages],[SimulatedPages],[InternalBenefit],[ExternalBenefit],[ValueOfMemory],[PeriodicFreedPages],[InternalFreedPages])) un'

if @type='RING_BUFFER_AUTO_ELEVATE_PFS_UP_LATCH'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''						,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(100)'') [Time]  
	,record.value(''(/Record/LatchRingBufferRecord/m_dbId)[1]''				,''varchar(100)'') [DBID]  
	,record.value(''(/Record/LatchRingBufferRecord/m_pgId)[1]''			,''varchar(100)'') [PageID]
	,record.value(''(/Record/LatchRingBufferRecord/m_latchType)[1]''	,''varchar(100)'') [LatchType]
	,record.value(''(/Record/LatchRingBufferRecord/m_recordType)[1]''		,''varchar(100)'') [RecordType]
FROM bt  
