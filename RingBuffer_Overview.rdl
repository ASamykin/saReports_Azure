if @type='RING_BUFFER_CONNECTIVITY'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''					,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(100)'') [Time]  
	,record.value(''(/Record/ConnectivityTraceRecord/RecordType)[1]''		,''varchar(100)'') [RecordType]  
	,record.value(''(/Record/ConnectivityTraceRecord/IsClient)[1]''		,''varchar(100)'') [IsClient]  
