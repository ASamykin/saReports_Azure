      </Fields>
    </DataSet>
    <DataSet Name="dsEventsGrouped">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (  
/*RING_BUFFER_CONNECTIVITY: get one pair time (timestamp) + RecordTime (datetime) to use it as a base to 
calculate date and time for all other events as  they only have "timestamp" column (bigint). 
*/
select top 1
	record.value('(Record/@time)[1]', 'bigint') [time_int],
	record.value('(Record/ConnectivityTraceRecord/RecordTime)[1]', 'datetime') [time_dt]
from
		( select cast(record as xml) as record
			from sys.dm_os_ring_buffers
			where ring_buffer_type = 'RING_BUFFER_CONNECTIVITY'
		) as tab
	)
, data2 as 
(
select 
	dateadd(ms,convert(bigint,rb.timestamp)-d.time_int,d.time_dt) [dt]
	,rb.ring_buffer_type [event]
from data d
cross join sys.dm_os_ring_buffers rb
)
select 
	convert(smalldatetime,dateadd(hh,datediff(hh,0,[dt]),0)) [dt_hh]
	,[event]
	,count(1) [nn_hh]
from data2
where [dt]&gt;=convert(date,getdate())
group by dateadd(hh,datediff(hh,0,[dt]),0) , [event]
order by 1 desc </CommandText>
      </Query>
      <Fields>
        <Field Name="dt_hh">
          <DataField>dt_hh</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="event">
          <DataField>event</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="nn_hh">
          <DataField>nn_hh</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
