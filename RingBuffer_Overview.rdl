	,record.value(''(/Record/ConnectivityTraceRecord/RecordTime)[1]''		,''varchar(100)'') [_RecordTime]  

	,record.value(''(/Record/ConnectivityTraceRecord/TdsBufInfo/InputBufError)[1]''		,''varchar(100)'') [TDS_InputBufError]  
