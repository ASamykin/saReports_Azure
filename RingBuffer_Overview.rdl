	,record.value(''(/Record/SchedulerMonitorEvent/SystemHealth/ProcessUtilization)[1]''		,''varchar(20)'') [ProcessUtilization]  
	,record.value(''(/Record/SchedulerMonitorEvent/SystemHealth/SystemIdle)[1]''		,''varchar(20)'') [SystemIdle]
	,record.value(''(/Record/SchedulerMonitorEvent/SystemHealth/UserModeTime)[1]''		,''varchar(20)'') [UserModeTime]
	,record.value(''(/Record/SchedulerMonitorEvent/SystemHealth/KernelModeTime)[1]''	,''varchar(20)'')	  [KernelModeTime]  
	,record.value(''(/Record/SchedulerMonitorEvent/SystemHealth/PageFaults)[1]''		,''varchar(20)'') [PageFaults]
