'

if @type='RING_BUFFER_XE_LOG'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''						,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(255)'') [Time]  
	,record.value(''(/Record/XE_LogRecord/@message)[1]'',''varchar(255)'') [Message]  
