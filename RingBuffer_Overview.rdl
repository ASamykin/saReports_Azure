	'

if @type='RING_BUFFER_EXCEPTION'
	set @part2='
	SELECT 
	ts
	,type
	,record.value(''(/Record/@id)[1]''					,''bigint'')		 [Record id] 
	,record.value(''(/Record/@time)[1]''					,''varchar(2048)'') [Time]  
	,record.value(''(/Record/Exception/Error)[1]''		,''varchar(2048)'') [Error]  
	,record.value(''(/Record/Exception/Severity)[1]''		,''varchar(2048)'') [Severity]  
	,record.value(''(/Record/Exception/State)[1]''		,''varchar(2048)'') [State]  
	,record.value(''(/Record/Exception/UserDefined)[1]''	,''varchar(2048)'') [UserDefined]  
	,record.value(''(/Record/Exception/Origin)[1]''		,''varchar(2048)'') [Origin]  
	,convert(varchar(2048),m.text) [Text]
FROM bt  
left join sys.messages m on bt.record.value(''(/Record/Exception/Error)[1]'',''int'')=m.message_id
)
select [nnn],
	ts,type,[Record ID] [rec],prop, val
from data   
