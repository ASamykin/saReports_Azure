    <DataSet Name="dsErrors">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@name">
            <Value>=Parameters!SessionName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @xml XML
select @xml = convert(xml,t.target_data)
from sys.dm_xe_database_sessions   s
join sys.dm_xe_database_session_targets t on t.event_session_address  = s.address
where s.name = @name
and t.target_name='ring_buffer' 

SELECT
	QP.value('(@timestamp)[1]', 'datetime2') AS [timestamp]
	,QP.value('(data[@name="category"]/text)[1]', 'varchar(255)') as [category]
	,QP.value('(data[@name="error_number"]/value)[1]', 'int') as [error_number]
	,QP.value('(data[@name="severity"]/value)[1]', 'int') as [severity]
	,QP.value('(data[@name="state"]/value)[1]', 'int') as [state]
	,QP.value('(data[@name="user_defined"]/value)[1]', 'bit') as [user_defined]
	
	,QP.value('(data[@name="destination"]/text)[1]', 'varchar(255)') as [destination]
	,QP.value('(data[@name="is_intercepted"]/value)[1]', 'bit') as [is_intercepted]
	,QP.value('(data[@name="message"]/value)[1]', 'nvarchar(512)') as [message]

	,QP.value('(action[@name="username"]/value)[1]', 'nvarchar(128)') as [username]
	,QP.value('(action[@name="session_id"]/value)[1]', 'int') as [session_id]
	,QP.value('(action[@name="query_hash"]/value)[1]', 'nvarchar(128)') as [query_hash]
	,QP.value('(action[@name="client_hostname"]/value)[1]', 'nvarchar(128)') as [client_hostname]
	,QP.value('(action[@name="client_app_name"]/value)[1]', 'nvarchar(128)') as [client_app_name]

	,QP.value('(action[@name="sql_text"]/value)[1]', 'varchar(max)') as [sql_text]
	,QP.value('(action[@name="tsql_stack"]/value)[1]', 'varchar(max)') as [tsql_stack]

	
FROM @xml.nodes('RingBufferTarget/event[@name=''error_reported'']') AS q(QP)
where 
QP.value('(data[@name="message"]/value)[1]', 'nvarchar(512)') not like 'Changed database context to %'
and 
QP.value('(data[@name="message"]/value)[1]', 'nvarchar(512)') &lt;&gt; 'Changed language setting to us_english.'</CommandText>
      </Query>
