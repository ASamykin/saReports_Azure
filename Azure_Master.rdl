        </Field>
        <Field Name="TermConn">
          <DataField>TermConn</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ThrotConn">
          <DataField>ThrotConn</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Desc">
          <Value>="Connections "
+chr(10)
+" - failed: "+cstr(Fields!FailConn.Value)
+chr(10)
+" - terminated: "+cstr(Fields!TermConn.Value)
+chr(10)
+" - throlttled: "+cstr(Fields!ThrotConn.Value)</Value>
        </Field>
      </Fields>
