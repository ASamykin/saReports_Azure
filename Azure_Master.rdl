    <DataSet Name="dsDBEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
with data as 
(
select
	database_name
	,event_type
	,count(1) [n]
from sys.event_log 
where 
end_time&gt;convert(date,getdate()-1)
and event_type&lt;&gt;'connection_failed'
group by 
	database_name
	,event_type
)
select 
	database_name [db]
	,sum(n) [events]
	,STRING_AGG(' - '+event_type+' '+quotename(convert(varchar(10),n)),char(10)) within group (order by n desc) [desc]
from data
group by database_name
option(recompile)




</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="events">
          <DataField>events</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
