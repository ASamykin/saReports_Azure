    <DataSet Name="dsUsers">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as (
select 
	type_desc
	,count(1) [n]
from sys.database_principals
where authentication_type&gt;0
and name &lt;&gt;'dbo'
group by type_desc
)
select
	sum(n) [ttl]
	,string_agg(type_desc+': '+convert(varchar(10),n),'; ') [text]
from data 
</CommandText>
      </Query>
      <Fields>
