    <DataSet Name="dsLastDTU">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with [data] as 
(
select 
	database_name [db]
	,dtu_limit [dtu]
	,ROW_NUMBER() over (partition by [database_name] order by [end_time] desc) [n]
from sys.resource_stats
)
select db,dtu
from [data] 
where n=1
option (recompile)
</CommandText>
