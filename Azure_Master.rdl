  <DataSets>
    <DataSet Name="dsResources">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>;with ranks as 
(
select database_name,sum(avg_cpu_percent+avg_data_io_percent+avg_log_write_percent+max_worker_percent+max_session_percent) [n]
from sys.resource_stats
where end_time&gt;=convert(date,getdate())
group by database_name 
)
select 
	r.n
	,convert(smalldatetime,rs.start_time) [start]
	,convert(smalldatetime,rs.end_time) [end]
	,convert(date,rs.end_time) [dt]
	,rs.database_name [db]
	,rs.sku
	,rs.storage_in_megabytes [storageMB]
	,rs.allocated_storage_in_megabytes [storageMBalloc]
	,rs.avg_cpu_percent [Cpu]
	,rs.avg_data_io_percent [DataIO]
	,rs.avg_log_write_percent [LogWr]
	,rs.max_worker_percent [MaxWorker]
	,rs.max_session_percent [MaxSess]
	,rs.dtu_limit [DTULimit]
	,d.create_date [created]
	,d.database_id [id]
from sys.resource_stats rs
join sys.databases d  on rs.database_name=d.name
join ranks r on rs.database_name=r.database_name
where rs.end_time&gt;=convert(date,getdate())
order by r.n
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="n">
          <DataField>n</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="start">
          <DataField>start</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="end">
          <DataField>end</DataField>
