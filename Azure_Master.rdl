    <DataSet Name="dsConnections">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	database_name [db]
	,sum(total_failure_count) [FailAll]
	,sum(connection_failure_count) [FailConn]
	,sum(terminated_connection_count) [TermConn]
	,sum(throttled_connection_count) [ThrotConn]
from sys.database_connection_stats 
where start_time&gt;=convert(date,getdate())
group by database_name 
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
