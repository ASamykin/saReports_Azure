    <DataSet Name="dsDBEvents">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with data as 
(
select
	database_name
	,event_type
	,count(1) [n]
from sys.event_log 
where 
end_time&gt;=convert(date,getdate())
and event_type not in ('connection_failed','deadlock')
group by 
	database_name
	,event_type
)
select 
	isnull(d.database_name,db.name) [db]
	,isnull(sum(n),0) [events]
	,STRING_AGG(' - '+event_type+' '+quotename(convert(varchar(10),n)),char(10)) within group (order by n desc) [desc]
from data d
right join sys.databases db on d.database_name=db.name
group by d.database_name,db.name
option(recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="events">
          <DataField>events</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsOperations">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
major_resource_id [resource]
,count(1) [events]
,avg(percent_complete) [perc]
,STRING_AGG(resource_type_desc+' '
			+quotename(convert(varchar(255),major_resource_id))
			+' operation '+operation
			+' is '+state_desc
			+' ('+convert(varchar(10),percent_complete)
			+'%), started at '+convert(varchar(20),start_time,106)+' '+convert(varchar(20),start_time,108) 
			+ ', '+case when state_desc='IN_PROGRESS' then 'running for ' else 'finished in 'end + convert(varchar(20),dateadd(ss,datediff(ss,start_time,last_modify_time),0),108)
			,char(10)) [desc]
,max(start_time) [last_start]
from sys.dm_operation_status
group by major_resource_id
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="resource">
          <DataField>resource</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="events">
          <DataField>events</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="perc">
          <DataField>perc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="last_start">
          <DataField>last_start</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsServiceChange">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>/*
with recs as 
(
	select 
		ROW_NUMBER() over (order by database_name, end_time asc) [n]
		,end_time 
		,dtu_limit
		,sku
		,database_name
	from sys.resource_stats
),
recs2 as 
(
	select 
		--t1.*,t2.*
		t1.database_name [db]
		,t2.end_time [date]
		,left(convert(varchar(20),t1.end_time,106),6)+' '+convert(varchar(20),t1.end_time,108)+' - '+left(convert(varchar(20),t2.end_time,106),6)+' '+convert(varchar(20),t2.end_time,108) [period]
		,convert(varchar(30),t1.sku)+' ('+convert(varchar(10),t1.dtu_limit)+ ') -&gt; '+convert(varchar(30),t2.sku)+' ('+convert(varchar(10),t2.dtu_limit)+')' [servicechange]
	from recs t1 
	join recs t2 on 
			t2.n=t1.n+1 
			and t2.database_name=t1.database_name
			and t2.dtu_limit&lt;&gt;t1.dtu_limit
)
select 
	db
	,STRING_AGG(' * '+[period]+ ' : '+[servicechange],char(10)) within group (order by [date] asc) [desc]
from recs2 
group by db
option (recompile)
*/
/*new version with LAG()*/
with data as (
select 
	database_name [db]
	,end_time [dt]
	,sku+' '+convert(varchar(10),dtu_limit) [service] 
	,lag(sku+' '+convert(varchar(10),dtu_limit),1,sku+' '+convert(varchar(10),dtu_limit)) over (partition by database_name order by end_time desc) [new]
from sys.resource_stats 
)
select 
		db
		,convert(varchar(20),dt,106)+' '+convert(varchar(10),dt,108) +' : '+[service]+' -&gt; '+[new] [desc]
from data 
where [service]&lt;&gt;[new]
order by 1,2 desc
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="desc">
          <DataField>desc</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDeadlocks">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 
	database_name [db]
	,convert(date,start_time) [dt]
	,count(1) [n]
	,sum(event_count) [ttl]
	,STRING_AGG(convert(varchar(20),start_time,108)+' - '+description+' '+quotename(convert(varchar(10),event_count)),char(10)) within group (order by start_time desc) [comments]
from sys.event_log 
where 
	event_type='deadlock' 
	and start_time&gt;=convert(date,getdate())
group by 
	database_name,
	convert(date,start_time)
option (recompile)</CommandText>
      </Query>
      <Fields>
        <Field Name="db">
          <DataField>db</DataField>
          <rd:UserDefined>true</rd:UserDefined>
